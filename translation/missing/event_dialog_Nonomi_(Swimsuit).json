            "LocalizeEN": "",
            "VoiceClipsJp": [
                "CH0092_EventLogin_Season_1"
            ]
        },
        {
            "CharacterId": 19009051,
            "DialogCategory": "UIEventLobby",
            "DialogCondition": "Enter",
