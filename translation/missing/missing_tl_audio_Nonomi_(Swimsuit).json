{
    "DataList": [
        {
            "CharacterId": 10044,
            "DialogCategory": "UITitle",
            "DialogCondition": "Idle",
            "GroupId": 1,
            "LocalizeKR": "블루 아카이브!",
            "LocalizeJP": "ブルーアーカイブ！",
