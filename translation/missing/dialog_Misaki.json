            "LocalizeEN": "",
            "VoiceClipsJp": []
        },
        {
            "CharacterId": 10041,
            "DialogCategory": "UILobbySpecial",
            "DialogCondition": "Idle",
            "GroupId": 2,
            "LocalizeKR": "나를, 걱정해서……?",
            "LocalizeJP": "私を、心配して……？",
            "LocalizeEN": "",
            "VoiceClipsJp": []
        },
        {
            "CharacterId": 10041,
            "DialogCategory": "UILobbySpecial",
            "DialogCondition": "Idle",
            "GroupId": 3,
            "LocalizeKR": "그런 하찮은 것에도\n신경 쓰다니……",
