            "LocalizeEN": "",
            "VoiceClipsJp": []
        },
        {
            "CharacterId": 10041,
            "DialogCategory": "UILobbySpecial",
            "DialogCondition": "Idle",
            "GroupId": 2,
            "LocalizeKR": "……나?",
