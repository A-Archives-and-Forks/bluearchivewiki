{
    "DataList": [
        {
            "CharacterId": 26002,
            "CharacterVoiceGroupId": 26002,
            "LocalizeCVGroup": "CVGroup_Formation_In_1",
            "DialogCategory": "Formation",
            "VoiceClip": "Juri_Formation_In_1",
            "LocalizeJP": "では、実力を発揮してみましょうか。",
            "LocalizeEN": "Let's show our strength."
        },
        {
            "CharacterId": 26002,
            "CharacterVoiceGroupId": 26002,
            "LocalizeCVGroup": "CVGroup_Formation_In_2",
            "DialogCategory": "Formation",
            "VoiceClip": "Juri_Formation_In_2",
            "LocalizeJP": "私にお任せください。",
            "LocalizeEN": "leave it to me"
        },
        {
            "CharacterId": 26002,
            "CharacterVoiceGroupId": 26002,
            "LocalizeCVGroup": "CVGroup_Battle_In_1",
            "DialogCategory": "Battle",
            "VoiceClip": "Juri_Battle_In_1",
            "LocalizeJP": "気をつけて、いってらっしゃい。",
            "LocalizeEN": "Take care of yourself and goodbye."
        },
        {
            "CharacterId": 26002,
            "CharacterVoiceGroupId": 26002,
            "LocalizeCVGroup": "CVGroup_Battle_In_2",
            "DialogCategory": "Battle",
            "VoiceClip": "Juri_Battle_In_2",
            "LocalizeJP": "補給なら心配しないでください。",
            "LocalizeEN": "Don't worry about replenishment."
        },
        {
            "CharacterId": 26002,
            "CharacterVoiceGroupId": 26002,
            "LocalizeCVGroup": "CVGroup_Battle_TSA_1",
            "DialogCategory": "Battle",
            "VoiceClip": "Juri_Battle_TSA_1",
            "LocalizeJP": "さあ。美味しいご飯が届きましたよ。",
            "LocalizeEN": "here we go. Delicious food has arrived."
        },
        {
            "CharacterId": 26002,
            "CharacterVoiceGroupId": 26002,
            "LocalizeCVGroup": "CVGroup_Battle_TSA_2",
            "DialogCategory": "Battle",
            "VoiceClip": "Juri_Battle_TSA_2",
            "LocalizeJP": "皆さん。お食事の時間にしましょう。",
            "LocalizeEN": "Everyone, let's make it time to eat"
        },
        {
            "CharacterId": 26002,
            "CharacterVoiceGroupId": 26002,
            "LocalizeCVGroup": "CVGroup_CommonSkill",
            "DialogCategory": "CommonSkill",
            "VoiceClip": "Juri_CommonSkill",
            "LocalizeJP": "私もお手伝いしますね。",
            "LocalizeEN": "I can help you too."
        },
        {
            "CharacterId": 26002,
            "CharacterVoiceGroupId": 26002,
            "LocalizeCVGroup": "CVGroup_ExSkill_1",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Juri_ExSkill_1",
            "LocalizeJP": "せーの…。あらら？",
            "LocalizeEN": "Well... Oops?"
        },
        {
            "CharacterId": 26002,
            "CharacterVoiceGroupId": 26002,
            "LocalizeCVGroup": "CVGroup_ExSkill_2",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Juri_ExSkill_2",
            "LocalizeJP": "このままひっくり返せば…。あれ？",
            "LocalizeEN": "If you just turn the frying pan over... Oh?"
        },
        {
            "CharacterId": 26002,
            "CharacterVoiceGroupId": 26002,
            "LocalizeCVGroup": "CVGroup_ExSkill_3",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Juri_ExSkill_3",
            "LocalizeJP": "これで完成です。…ちょっと待って！？",
            "LocalizeEN": "This is completed. …wait a minute! ?"
        },
        {
            "CharacterId": 26002,
            "CharacterVoiceGroupId": 26002,
            "LocalizeCVGroup": "CVGroup_ExSkill_Level_1",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Juri_ExSkill_Level_1",
            "LocalizeJP": "あ、あれ？",
            "LocalizeEN": ""
        },
        {
            "CharacterId": 26002,
            "CharacterVoiceGroupId": 26002,
            "LocalizeCVGroup": "CVGroup_ExSkill_Level_2",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Juri_ExSkill_Level_2",
            "LocalizeJP": "皆さんが食べちゃダメなのに…。",
            "LocalizeEN": "Even though you can't eat it."
        },
        {
            "CharacterId": 26002,
            "CharacterVoiceGroupId": 26002,
            "LocalizeCVGroup": "CVGroup_ExSkill_Level_3",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Juri_ExSkill_Level_3",
            "LocalizeJP": "皆さんの分ではありませーん！",
            "LocalizeEN": "It's not for you!"
        },
        {
            "CharacterId": 26002,
            "CharacterVoiceGroupId": 26002,
            "LocalizeCVGroup": "CVGroup_Growup_1",
            "DialogCategory": "Growup",
            "VoiceClip": "Juri_Growup_1",
            "LocalizeJP": "これでもっとおいしいご飯が作れるようになれますでしょうか。",
            "LocalizeEN": "Will I be able to make more delicious rice with this?"
        },
        {
            "CharacterId": 26002,
            "CharacterVoiceGroupId": 26002,
            "LocalizeCVGroup": "CVGroup_Growup_2",
            "DialogCategory": "Growup",
            "VoiceClip": "Juri_Growup_2",
            "LocalizeJP": "お料理上手になるためにも、体力は必須です。",
            "LocalizeEN": "Physical fitness is essential to become a good cook."
        },
        {
            "CharacterId": 26002,
            "CharacterVoiceGroupId": 26002,
            "LocalizeCVGroup": "CVGroup_Growup_3",
            "DialogCategory": "Growup",
            "VoiceClip": "Juri_Growup_3",
            "LocalizeJP": "うふふ、なんだかお料理がしたくなりますね。",
            "LocalizeEN": "It makes me want to cook."
        },
        {
            "CharacterId": 26002,
            "CharacterVoiceGroupId": 26002,
            "LocalizeCVGroup": "CVGroup_Growup_4",
            "DialogCategory": "Growup",
            "VoiceClip": "Juri_Growup_4",
            "LocalizeJP": "もっと美味しいお料理を作るために、頑張ります。",
            "LocalizeEN": "I will do my best to make more delicious dishes."
        },
        {
            "CharacterId": 26002,
            "CharacterVoiceGroupId": 26002,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_1",
            "DialogCategory": "Relationship",
            "VoiceClip": "Juri_Relationship_Up_1",
            "LocalizeJP": "先生と一緒に過ごす時間は、お料理をしてる時と同じくらい楽しいです。",
            "LocalizeEN": "Spending time with Sensei is as fun as cooking."
        },
        {
            "CharacterId": 26002,
            "CharacterVoiceGroupId": 26002,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_2",
            "DialogCategory": "Relationship",
            "VoiceClip": "Juri_Relationship_Up_2",
            "LocalizeJP": "最高の香辛料は…、まさしく愛です。",
            "LocalizeEN": "The best spice is...love."
        },
        {
            "CharacterId": 26002,
            "CharacterVoiceGroupId": 26002,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_3",
            "DialogCategory": "Relationship",
            "VoiceClip": "Juri_Relationship_Up_3",
            "LocalizeJP": "この幸せを味で表現するためには、どんな食材を入れればいいのか…、悩みますね。",
            "LocalizeEN": "I'm worried about what kind of ingredients I should put in order to express this happiness with taste."
        },
        {
            "CharacterId": 26002,
            "CharacterVoiceGroupId": 26002,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_4",
            "DialogCategory": "Relationship",
            "VoiceClip": "Juri_Relationship_Up_4",
            "LocalizeJP": "先生と一緒に食べるお料理なら、どのようなお料理でも美味しいです。",
            "LocalizeEN": "Any kind of food is delicious as long as you eat it with Sensei."
        }
    ]
}
