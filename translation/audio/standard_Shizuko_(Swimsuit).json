{
    "DataList": [
        {
            "CharacterId": 26008,
            "CharacterVoiceGroupId": 26008,
            "LocalizeCVGroup": null,
            "DialogCategory": "EventLocation",
            "VoiceClip": "Shizuko_(Swimsuit)_EventLocation_1",
            "LocalizeJP": "このままどんどん規模を拡大していきましょう！",
            "LocalizeEN": "We just don-don't stop expanding at this rate!"
        },
        {
            "CharacterId": 26008,
            "CharacterVoiceGroupId": 26008,
            "LocalizeCVGroup": null,
            "DialogCategory": "EventLocation",
            "VoiceClip": "Shizuko_(Swimsuit)_EventLocation_2",
            "LocalizeJP": "ふふふん、百夜堂が、どんどん大きく…。さあ！この勢いで行きましょう！",
            "LocalizeEN": ""
        },
        {
            "CharacterId": 26008,
            "CharacterVoiceGroupId": 26008,
            "LocalizeCVGroup": null,
            "DialogCategory": "EventLocation",
            "VoiceClip": "Shizuko_(Swimsuit)_EventLocation_3",
            "LocalizeJP": "ふふん！順調に成長していってる感じですねー！",
            "LocalizeEN": ""
        },
        {
            "CharacterId": 26008,
            "CharacterVoiceGroupId": 26008,
            "LocalizeCVGroup": null,
            "DialogCategory": "EventLocation",
            "VoiceClip": "Shizuko_(Swimsuit)_EventLocation_Act_1",
            "LocalizeJP": "百夜堂、営業開始です！",
            "LocalizeEN": ""
        },
        {
            "CharacterId": 26008,
            "CharacterVoiceGroupId": 26008,
            "LocalizeCVGroup": null,
            "DialogCategory": "EventLocation",
            "VoiceClip": "Shizuko_(Swimsuit)_EventLocation_Act_2",
            "LocalizeJP": "お客様をお迎えするときは、笑顔で！",
            "LocalizeEN": ""
        },
        {
            "CharacterId": 26008,
            "CharacterVoiceGroupId": 26008,
            "LocalizeCVGroup": null,
            "DialogCategory": "EventLocation",
            "VoiceClip": "Shizuko_(Swimsuit)_EventLocation_Act_3",
            "LocalizeJP": "必要なものがあれば、何でもおっしゃってください！",
            "LocalizeEN": ""
        },
        {
            "CharacterId": 26008,
            "CharacterVoiceGroupId": 26008,
            "LocalizeCVGroup": "CVGroup_Formation_In_1",
            "DialogCategory": "Formation",
            "VoiceClip": "Shizuko_(Swimsuit)_Formation_In_1",
            "LocalizeJP": "百夜堂出張しま～す。",
            "LocalizeEN": "I'm going on a business trip to Momoyodo."
        },
        {
            "CharacterId": 26008,
            "CharacterVoiceGroupId": 26008,
            "LocalizeCVGroup": "CVGroup_Formation_In_2",
            "DialogCategory": "Formation",
            "VoiceClip": "Shizuko_(Swimsuit)_Formation_In_2",
            "LocalizeJP": "百夜堂開店で～す。",
            "LocalizeEN": ""
        },
        {
            "CharacterId": 26008,
            "CharacterVoiceGroupId": 26008,
            "LocalizeCVGroup": "CVGroup_CommonSkill",
            "DialogCategory": "CommonSkill",
            "VoiceClip": "Shizuko_(Swimsuit)_CommonSkill",
            "LocalizeJP": "出張版百夜堂名物！\n「河和たこ焼き」ですよ～！",
            "LocalizeEN": "It's a business trip version of Momoyodo, Kawawa Takoyaki!"
        },
        {
            "CharacterId": 26008,
            "CharacterVoiceGroupId": 26008,
            "LocalizeCVGroup": "CVGroup_ExSkill_1",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Shizuko_(Swimsuit)_ExSkill_1",
            "LocalizeJP": "百夜堂出張版、海の家ですよ～！",
            "LocalizeEN": "Momoyodo business trip version, a beach house!"
        },
        {
            "CharacterId": 26008,
            "CharacterVoiceGroupId": 26008,
            "LocalizeCVGroup": "CVGroup_ExSkill_2",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Shizuko_(Swimsuit)_ExSkill_2",
            "LocalizeJP": "さあ、お召し上がりください！",
            "LocalizeEN": "Go ahead and eat!"
        },
        {
            "CharacterId": 26008,
            "CharacterVoiceGroupId": 26008,
            "LocalizeCVGroup": "CVGroup_ExSkill_3",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Shizuko_(Swimsuit)_ExSkill_3",
            "LocalizeJP": "さあ、かき氷はいかがでしょうか？",
            "LocalizeEN": "How about some shaved ice?"
        },
        {
            "CharacterId": 26008,
            "CharacterVoiceGroupId": 26008,
            "LocalizeCVGroup": "CVGroup_ExSkill_Level_1",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Shizuko_(Swimsuit)_ExSkill_Level_1",
            "LocalizeJP": "夏と言ったら、やっぱりこれですよね！",
            "LocalizeEN": "This is what summer is all about."
        },
        {
            "CharacterId": 26008,
            "CharacterVoiceGroupId": 26008,
            "LocalizeCVGroup": "CVGroup_ExSkill_Level_2",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Shizuko_(Swimsuit)_ExSkill_Level_2",
            "LocalizeJP": "暑いの暑いの、飛んでけ～！",
            "LocalizeEN": "It's hot, it's hot, fly away."
        },
        {
            "CharacterId": 26008,
            "CharacterVoiceGroupId": 26008,
            "LocalizeCVGroup": "CVGroup_ExSkill_Level_3",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Shizuko_(Swimsuit)_ExSkill_Level_3",
            "LocalizeJP": "まだまだい～っぱいありますよ！",
            "LocalizeEN": "There are still many more."
        },
        {
            "CharacterId": 26008,
            "CharacterVoiceGroupId": 26008,
            "LocalizeCVGroup": "CVGroup_Growup_1",
            "DialogCategory": "Growup",
            "VoiceClip": "Shizuko_(Swimsuit)_Growup_1",
            "LocalizeJP": "先生、どうですか？私のこと、ちょっとは見直しましたか？\nあ、そ、そうですか……",
            "LocalizeEN": "Sensei, have you reconsidered me at all? ……Oh really…."
        },
        {
            "CharacterId": 26008,
            "CharacterVoiceGroupId": 26008,
            "LocalizeCVGroup": "CVGroup_Growup_2",
            "DialogCategory": "Growup",
            "VoiceClip": "Shizuko_(Swimsuit)_Growup_2",
            "LocalizeJP": "さあ、また強くなってしまいました！\n今のうちがお買い得かもしれませんよ～？",
            "LocalizeEN": "I became strong again. It may be a bargain now."
        },
        {
            "CharacterId": 26008,
            "CharacterVoiceGroupId": 26008,
            "LocalizeCVGroup": "CVGroup_Growup_3",
            "DialogCategory": "Growup",
            "VoiceClip": "Shizuko_(Swimsuit)_Growup_3",
            "LocalizeJP": "私、まだまだ成長できそうです！\nふふっ、これは期待できますね？",
            "LocalizeEN": "I think I can still grow. Hehe, we can look forward to this!"
        },
        {
            "CharacterId": 26008,
            "CharacterVoiceGroupId": 26008,
            "LocalizeCVGroup": "CVGroup_Growup_4",
            "DialogCategory": "Growup",
            "VoiceClip": "Shizuko_(Swimsuit)_Growup_4",
            "LocalizeJP": "私、まだまだ進化し続けてますよ～！\n先生も、しっかりついてきてくださいね？",
            "LocalizeEN": "I'm still evolving, so please follow me."
        },
        {
            "CharacterId": 26008,
            "CharacterVoiceGroupId": 26008,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_1",
            "DialogCategory": "Relationship",
            "VoiceClip": "Shizuko_(Swimsuit)_Relationship_Up_1",
            "LocalizeJP": "大事ではない関係なんてありませんが、\nそれでも先生との関係は何だか、特別な感じがします",
            "LocalizeEN": "It's not important, there's no such thing as a relationship, but still, there's something special about my relationship with my teacher."
        },
        {
            "CharacterId": 26008,
            "CharacterVoiceGroupId": 26008,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_2",
            "DialogCategory": "Relationship",
            "VoiceClip": "Shizuko_(Swimsuit)_Relationship_Up_2",
            "LocalizeJP": "営業も大事ですが、その後は…、えっと……\nい、いえ！やっぱり何でもないです！",
            "LocalizeEN": "Sales is important, but after that...well... home! It's nothing after all!"
        },
        {
            "CharacterId": 26008,
            "CharacterVoiceGroupId": 26008,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_3",
            "DialogCategory": "Relationship",
            "VoiceClip": "Shizuko_(Swimsuit)_Relationship_Up_3",
            "LocalizeJP": "（先生とのこういう時間がもっと続けばいいのに……\n夏が終わったら、無くなっちゃうのかな）",
            "LocalizeEN": "(I wish I could spend more time like this with my teacher...I wonder if it will disappear once the summer is over.)"
        },
        {
            "CharacterId": 26008,
            "CharacterVoiceGroupId": 26008,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_4",
            "DialogCategory": "Relationship",
            "VoiceClip": "Shizuko_(Swimsuit)_Relationship_Up_4",
            "LocalizeJP": "夏の海って、不思議ですね。楽しくて、ワクワクして\n……それでいて、どこか切ないところが。",
            "LocalizeEN": "The sea in summer is strange. It's fun and exciting, and yet...it's also sad somehow."
        }
    ]
}
