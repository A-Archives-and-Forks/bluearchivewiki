{
    "DataList": [
        {
            "CharacterId": 13012,
            "CharacterVoiceGroupId": 13012,
            "LocalizeCVGroup": "CVGroup_Formation_In_1",
            "DialogCategory": "Formation",
            "VoiceClip": "Kirino_Formation_In_1",
            "LocalizeJP": "出動ですか？",
            "LocalizeEN": "Am I dispatched?"
        },
        {
            "CharacterId": 13012,
            "CharacterVoiceGroupId": 13012,
            "LocalizeCVGroup": "CVGroup_Formation_In_2",
            "DialogCategory": "Formation",
            "VoiceClip": "Kirino_Formation_In_2",
            "LocalizeJP": "準備完了です。",
            "LocalizeEN": "Ready."
        },
        {
            "CharacterId": 13012,
            "CharacterVoiceGroupId": 13012,
            "LocalizeCVGroup": "CVGroup_Formation_Select",
            "DialogCategory": "Formation",
            "VoiceClip": "Kirino_Formation_Select",
            "LocalizeJP": "ちょ、ちょ、ちょっと待ってください！",
            "LocalizeEN": "P-Please wait a second!"
        },
        {
            "CharacterId": 13012,
            "CharacterVoiceGroupId": 13012,
            "LocalizeCVGroup": "CVGroup_Tactic_Defeat_1",
            "DialogCategory": "Tactic",
            "VoiceClip": "Kirino_Tactic_Defeat_1",
            "LocalizeJP": "正義は敗北…、しません！…うう。",
            "LocalizeEN": "Justice is defeated...I won't! …Ah."
        },
        {
            "CharacterId": 13012,
            "CharacterVoiceGroupId": 13012,
            "LocalizeCVGroup": "CVGroup_Tactic_Defeat_2",
            "DialogCategory": "Tactic",
            "VoiceClip": "Kirino_Tactic_Defeat_2",
            "LocalizeJP": "本官のミスです。",
            "LocalizeEN": "It was my mistake."
        },
        {
            "CharacterId": 13012,
            "CharacterVoiceGroupId": 13012,
            "LocalizeCVGroup": "CVGroup_Tactic_In_1",
            "DialogCategory": "Tactic",
            "VoiceClip": "Kirino_Tactic_In_1",
            "LocalizeJP": "市民の安全を最優先でいきましょう。",
            "LocalizeEN": "Let's put public safety first."
        },
        {
            "CharacterId": 13012,
            "CharacterVoiceGroupId": 13012,
            "LocalizeCVGroup": "CVGroup_Tactic_In_2",
            "DialogCategory": "Tactic",
            "VoiceClip": "Kirino_Tactic_In_2",
            "LocalizeJP": "犯罪者はどこですか",
            "LocalizeEN": "Where are the criminals?"
        },
        {
            "CharacterId": 13012,
            "CharacterVoiceGroupId": 13012,
            "LocalizeCVGroup": "CVGroup_Tactic_Victory_1",
            "DialogCategory": "Tactic",
            "VoiceClip": "Kirino_Tactic_Victory_1",
            "LocalizeJP": "市民の皆さん、キヴォトスは今日も安全です。",
            "LocalizeEN": "Citizens, Kivotos is safe today."
        },
        {
            "CharacterId": 13012,
            "CharacterVoiceGroupId": 13012,
            "LocalizeCVGroup": "CVGroup_Tactic_Victory_2",
            "DialogCategory": "Tactic",
            "VoiceClip": "Kirino_Tactic_Victory_2",
            "LocalizeJP": "今日もパトロールにご協力してくださって、ありがとうございます。",
            "LocalizeEN": "Thank you for your cooperation with the patrol today."
        },
        {
            "CharacterId": 13012,
            "CharacterVoiceGroupId": 13012,
            "LocalizeCVGroup": "CVGroup_Battle_Buffed_1",
            "DialogCategory": "Battle",
            "VoiceClip": "Kirino_Battle_Buffed_1",
            "LocalizeJP": "ご支援感謝します。",
            "LocalizeEN": "Thank you for your support."
        },
        {
            "CharacterId": 13012,
            "CharacterVoiceGroupId": 13012,
            "LocalizeCVGroup": "CVGroup_Battle_BuffSelf_1",
            "DialogCategory": "Battle",
            "VoiceClip": "Kirino_Battle_BuffSelf_1",
            "LocalizeJP": "本官が先陣を切ります。",
            "LocalizeEN": "I will take the lead."
        },
        {
            "CharacterId": 13012,
            "CharacterVoiceGroupId": 13012,
            "LocalizeCVGroup": "CVGroup_Battle_Covered_1",
            "DialogCategory": "Battle",
            "VoiceClip": "Kirino_Battle_Covered_1",
            "LocalizeJP": "隠れます。",
            "LocalizeEN": "Taking cover!"
        },
        {
            "CharacterId": 13012,
            "CharacterVoiceGroupId": 13012,
            "LocalizeCVGroup": "CVGroup_Battle_Damage_1",
            "DialogCategory": "Battle",
            "VoiceClip": "Kirino_Battle_Damage_1",
            "LocalizeJP": "",
            "LocalizeEN": ""
        },
        {
            "CharacterId": 13012,
            "CharacterVoiceGroupId": 13012,
            "LocalizeCVGroup": "CVGroup_Battle_Damage_2",
            "DialogCategory": "Battle",
            "VoiceClip": "Kirino_Battle_Damage_2",
            "LocalizeJP": "",
            "LocalizeEN": ""
        },
        {
            "CharacterId": 13012,
