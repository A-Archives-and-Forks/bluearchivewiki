{
    "DataList": [
        {
            "CharacterId": 20022,
            "CharacterVoiceGroupId": 20022,
            "LocalizeCVGroup": "CVGroup_Formation_In_1",
            "DialogCategory": "Formation",
            "VoiceClip": "Fuuka_(New_Year)_Formation_In_1",
            "LocalizeJP": "準備はできています。",
            "LocalizeEN": "I am ready."
        },
        {
            "CharacterId": 20022,
            "CharacterVoiceGroupId": 20022,
            "LocalizeCVGroup": "CVGroup_Formation_In_2",
            "DialogCategory": "Formation",
            "VoiceClip": "Fuuka_(New_Year)_Formation_In_2",
            "LocalizeJP": "お正月料理をご用意しますね。",
            "LocalizeEN": "I will prepare a New Year's dish."
        },
        {
            "CharacterId": 20022,
            "CharacterVoiceGroupId": 20022,
            "LocalizeCVGroup": "CVGroup_CommonSkill",
            "DialogCategory": "CommonSkill",
            "VoiceClip": "Fuuka_(New_Year)_CommonSkill",
            "LocalizeJP": "気をつけてください。",
            "LocalizeEN": "Please be careful."
        },
        {
            "CharacterId": 20022,
            "CharacterVoiceGroupId": 20022,
            "LocalizeCVGroup": "CVGroup_CommonTSASkill",
            "DialogCategory": "CommonTSASkill",
            "VoiceClip": "Fuuka_(New_Year)_CommonTSASkill",
            "LocalizeJP": "どうでしたか？",
            "LocalizeEN": "How was it?"
        },
        {
            "CharacterId": 20022,
            "CharacterVoiceGroupId": 20022,
            "LocalizeCVGroup": "CVGroup_ExSkill_1",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Fuuka_(New_Year)_ExSkill_1",
            "LocalizeJP": "それでは行きましょうか。",
            "LocalizeEN": "Shall we go then?"
        },
        {
            "CharacterId": 20022,
            "CharacterVoiceGroupId": 20022,
            "LocalizeCVGroup": "CVGroup_ExSkill_2",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Fuuka_(New_Year)_ExSkill_2",
            "LocalizeJP": "心を込めて作りました。",
            "LocalizeEN": "I made it with all my heart."
        },
        {
            "CharacterId": 20022,
            "CharacterVoiceGroupId": 20022,
            "LocalizeCVGroup": "CVGroup_ExSkill_3",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Fuuka_(New_Year)_ExSkill_3",
            "LocalizeJP": "出発しますね。",
            "LocalizeEN": "I'm heading there."
        },
        {
            "CharacterId": 20022,
            "CharacterVoiceGroupId": 20022,
            "LocalizeCVGroup": "CVGroup_ExSkill_Level_1",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Fuuka_(New_Year)_ExSkill_Level_1",
            "LocalizeJP": "給食部の特製おせちです。",
            "LocalizeEN": "This is a special osechi from the school lunch club."
        },
        {
            "CharacterId": 20022,
            "CharacterVoiceGroupId": 20022,
            "LocalizeCVGroup": "CVGroup_ExSkill_Level_2",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Fuuka_(New_Year)_ExSkill_Level_2",
            "LocalizeJP": "まごころいっぱいですよ。",
            "LocalizeEN": "It's full of sincerity."
        },
        {
            "CharacterId": 20022,
            "CharacterVoiceGroupId": 20022,
            "LocalizeCVGroup": "CVGroup_ExSkill_Level_3",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Fuuka_(New_Year)_ExSkill_Level_3",
            "LocalizeJP": "おかわりならたくさんありますよ。",
            "LocalizeEN": "There are plenty of refills."
        },
        {
            "CharacterId": 20022,
            "CharacterVoiceGroupId": 20022,
            "LocalizeCVGroup": "CVGroup_Growup_1",
            "DialogCategory": "Growup",
            "VoiceClip": "Fuuka_(New_Year)_Growup_1",
            "LocalizeJP": "いつもありがとうございます、先生。",
            "LocalizeEN": "Thanks as always, Sensei."
        },
        {
            "CharacterId": 20022,
            "CharacterVoiceGroupId": 20022,
            "LocalizeCVGroup": "CVGroup_Growup_2",
            "DialogCategory": "Growup",
            "VoiceClip": "Fuuka_(New_Year)_Growup_2",
            "LocalizeJP": "これを私に…。頂いても良いのでしょうか？",
            "LocalizeEN": "Are you going to give me this...? Can I really have it?"
        },
        {
            "CharacterId": 20022,
            "CharacterVoiceGroupId": 20022,
            "LocalizeCVGroup": "CVGroup_Growup_3",
            "DialogCategory": "Growup",
            "VoiceClip": "Fuuka_(New_Year)_Growup_3",
            "LocalizeJP": "どうしましょう、お返しできるものが何も…、え？すでにもらっている？",
            "LocalizeEN": "What should I do? I have nothing to give back... eh? I already have?"
        },
        {
            "CharacterId": 20022,
            "CharacterVoiceGroupId": 20022,
            "LocalizeCVGroup": "CVGroup_Growup_4",
            "DialogCategory": "Growup",
            "VoiceClip": "Fuuka_(New_Year)_Growup_4",
            "LocalizeJP": "こんなに成長できるだなんて。全部先生のおかげです、ありがとうございます。",
            "LocalizeEN": "To think I could grow so much. It's all thanks to you, Sensei."
        },
        {
            "CharacterId": 20022,
            "CharacterVoiceGroupId": 20022,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_1",
            "DialogCategory": "Relationship",
            "VoiceClip": "Fuuka_(New_Year)_Relationship_Up_1",
            "LocalizeJP": "何かあったらいつでも声をかけてくださいね。…いつでも、ですよ。",
            "LocalizeEN": "Please feel free to call me if you need anything. ...Anytime."
        },
        {
            "CharacterId": 20022,
            "CharacterVoiceGroupId": 20022,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_2",
            "DialogCategory": "Relationship",
            "VoiceClip": "Fuuka_(New_Year)_Relationship_Up_2",
            "LocalizeJP": "先生と一緒に過ごす日々は、私にとってかけがえのない時間なんです。",
            "LocalizeEN": "The days me and Sensei spend together are irreplaceable to me."
        },
        {
            "CharacterId": 20022,
            "CharacterVoiceGroupId": 20022,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_3",
            "DialogCategory": "Relationship",
            "VoiceClip": "Fuuka_(New_Year)_Relationship_Up_3",
            "LocalizeJP": "先生？えへへ、呼んでみたかっただけです。",
            "LocalizeEN": "Sensei? Ehehe, I just wanted to say your name."
        },
        {
            "CharacterId": 20022,
            "CharacterVoiceGroupId": 20022,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_4",
            "DialogCategory": "Relationship",
            "VoiceClip": "Fuuka_(New_Year)_Relationship_Up_4",
            "LocalizeJP": "先生に、伝えたい言葉があるんです。いつか伝えるので待っていてくれますか？",
            "LocalizeEN": "Sensei, there's one thing I have to say to you. I'll say it someday so, until then will you wait for me?"
        }
    ]
}
