{
    "DataList": [
        {
            "CharacterId": 16011,
            "CharacterVoiceGroupId": 16011,
            "LocalizeCVGroup": "CVGroup_Formation_In_1",
            "DialogCategory": "Formation",
            "VoiceClip": "Hasumi_(Sportswear)_Formation_In_1",
            "LocalizeJP": "お任せください。",
            "LocalizeEN": "Leave it to me."
        },
        {
            "CharacterId": 16011,
            "CharacterVoiceGroupId": 16011,
            "LocalizeCVGroup": "CVGroup_Formation_In_2",
            "DialogCategory": "Formation",
            "VoiceClip": "Hasumi_(Sportswear)_Formation_In_2",
            "LocalizeJP": "お待ちしておりました。\n先生、ご指示を。",
            "LocalizeEN": "I have been waiting. Sensei, your command?"
        },
        {
            "CharacterId": 16011,
            "CharacterVoiceGroupId": 16011,
            "LocalizeCVGroup": "CVGroup_Formation_Select",
            "DialogCategory": "Formation",
            "VoiceClip": "Hasumi_(Sportswear)_Formation_Select",
            "LocalizeJP": "せん……せい？",
            "LocalizeEN": "Sen... sei...?"
        },
        {
            "CharacterId": 16011,
            "CharacterVoiceGroupId": 16011,
            "LocalizeCVGroup": "CVGroup_Tactic_Defeat_1",
            "DialogCategory": "Tactic",
            "VoiceClip": "Hasumi_(Sportswear)_Tactic_Defeat_1",
            "LocalizeJP": "申し訳ございません。不手際があったようです。",
            "LocalizeEN": "I'm sorry. It seems there was a mistake."
        },
        {
            "CharacterId": 16011,
            "CharacterVoiceGroupId": 16011,
            "LocalizeCVGroup": "CVGroup_Tactic_Defeat_2",
            "DialogCategory": "Tactic",
            "VoiceClip": "Hasumi_(Sportswear)_Tactic_Defeat_2",
            "LocalizeJP": "これでは選手失格ですね。",
            "LocalizeEN": "I guess this disqualifies me."
        },
        {
            "CharacterId": 16011,
            "CharacterVoiceGroupId": 16011,
            "LocalizeCVGroup": "CVGroup_Tactic_In_1",
            "DialogCategory": "Tactic",
            "VoiceClip": "Hasumi_(Sportswear)_Tactic_In_1",
            "LocalizeJP": "試合開始です。",
            "LocalizeEN": "The match has begun."
        },
        {
            "CharacterId": 16011,
            "CharacterVoiceGroupId": 16011,
            "LocalizeCVGroup": "CVGroup_Tactic_In_2",
            "DialogCategory": "Tactic",
            "VoiceClip": "Hasumi_(Sportswear)_Tactic_In_2",
            "LocalizeJP": "最善を尽くします。",
            "LocalizeEN": "I will do my best."
        },
        {
            "CharacterId": 16011,
            "CharacterVoiceGroupId": 16011,
            "LocalizeCVGroup": "CVGroup_Tactic_Victory_1",
            "DialogCategory": "Tactic",
            "VoiceClip": "Hasumi_(Sportswear)_Tactic_Victory_1",
            "LocalizeJP": "素晴らしい作戦でした、先生。",
            "LocalizeEN": "It was a great strategy, Sensei."
        },
        {
            "CharacterId": 16011,
            "CharacterVoiceGroupId": 16011,
            "LocalizeCVGroup": "CVGroup_Tactic_Victory_2",
            "DialogCategory": "Tactic",
            "VoiceClip": "Hasumi_(Sportswear)_Tactic_Victory_2",
            "LocalizeJP": "たまにはこういう格好も、動きやすくて良いですね。",
            "LocalizeEN": "I guess is nice to dress lightly like this every now and then."
        },
        {
            "CharacterId": 16011,
            "CharacterVoiceGroupId": 16011,
            "LocalizeCVGroup": "CVGroup_Battle_Buffed_1",
            "DialogCategory": "Battle",
            "VoiceClip": "Hasumi_(Sportswear)_Battle_Buffed_1",
            "LocalizeJP": "声援、感謝します。",
            "LocalizeEN": "Thank you for your encouragement."
        },
        {
            "CharacterId": 16011,
            "CharacterVoiceGroupId": 16011,
            "LocalizeCVGroup": "CVGroup_Battle_BuffSelf_1",
            "DialogCategory": "Battle",
            "VoiceClip": "Hasumi_(Sportswear)_Battle_BuffSelf_1",
            "LocalizeJP": "もう少し頑張ってみます。",
            "LocalizeEN": "I'll try a little harder."
        },
        {
            "CharacterId": 16011,
            "CharacterVoiceGroupId": 16011,
            "LocalizeCVGroup": "CVGroup_Battle_Covered_1",
            "DialogCategory": "Battle",
            "VoiceClip": "Hasumi_(Sportswear)_Battle_Covered_1",
            "LocalizeJP": "隠れます。",
            "LocalizeEN": "Hiding."
        },
        {
            "CharacterId": 16011,
            "CharacterVoiceGroupId": 16011,
            "LocalizeCVGroup": "CVGroup_Battle_Damage_1",
            "DialogCategory": "Battle",
            "VoiceClip": "Hasumi_(Sportswear)_Battle_Damage_1",
            "LocalizeJP": "",
            "LocalizeEN": ""
        },
        {
            "CharacterId": 16011,
            "CharacterVoiceGroupId": 16011,
            "LocalizeCVGroup": "CVGroup_Battle_Damage_2",
            "DialogCategory": "Battle",
            "VoiceClip": "Hasumi_(Sportswear)_Battle_Damage_2",
            "LocalizeJP": "",
            "LocalizeEN": ""
        },
        {
            "CharacterId": 16011,
            "CharacterVoiceGroupId": 16011,
            "LocalizeCVGroup": "CVGroup_Battle_Damage_3",
            "DialogCategory": "Battle",
            "VoiceClip": "Hasumi_(Sportswear)_Battle_Damage_3",
            "LocalizeJP": "",
            "LocalizeEN": ""
        },
        {
            "CharacterId": 16011,
            "CharacterVoiceGroupId": 16011,
            "LocalizeCVGroup": "CVGroup_Battle_Defense_1",
            "DialogCategory": "Battle",
            "VoiceClip": "Hasumi_(Sportswear)_Battle_Defense_1",
            "LocalizeJP": "残念です。もっと頑張ってください。",
            "LocalizeEN": "Too bad. Try a little harder."
        },
        {
            "CharacterId": 16011,
            "CharacterVoiceGroupId": 16011,
            "LocalizeCVGroup": "CVGroup_Battle_In_1",
            "DialogCategory": "Battle",
            "VoiceClip": "Hasumi_(Sportswear)_Battle_In_1",
            "LocalizeJP": "それではまず、走ってみましょうか。",
            "LocalizeEN": "First, should we do a few laps?"
        },
        {
            "CharacterId": 16011,
            "CharacterVoiceGroupId": 16011,
            "LocalizeCVGroup": "CVGroup_Battle_In_2",
            "DialogCategory": "Battle",
            "VoiceClip": "Hasumi_(Sportswear)_Battle_In_2",
            "LocalizeJP": "正々堂々と参ります。",
            "LocalizeEN": "Let's fight fair and square."
        },
        {
            "CharacterId": 16011,
            "CharacterVoiceGroupId": 16011,
            "LocalizeCVGroup": "CVGroup_Battle_Move_1",
            "DialogCategory": "Battle",
            "VoiceClip": "Hasumi_(Sportswear)_Battle_Move_1",
            "LocalizeJP": "急ぎましょう。",
            "LocalizeEN": "Let's hurry."
        },
        {
            "CharacterId": 16011,
            "CharacterVoiceGroupId": 16011,
            "LocalizeCVGroup": "CVGroup_Battle_Move_2",
            "DialogCategory": "Battle",
            "VoiceClip": "Hasumi_(Sportswear)_Battle_Move_2",
            "LocalizeJP": "まだ終わっていません。",
            "LocalizeEN": "It's not over yet."
        },
        {
            "CharacterId": 16011,
            "CharacterVoiceGroupId": 16011,
            "LocalizeCVGroup": "CVGroup_Battle_Recovery_1",
            "DialogCategory": "Battle",
            "VoiceClip": "Hasumi_(Sportswear)_Battle_Recovery_1",
            "LocalizeJP": "ありがとうございます。",
            "LocalizeEN": "Thank you."
        },
        {
            "CharacterId": 16011,
            "CharacterVoiceGroupId": 16011,
            "LocalizeCVGroup": "CVGroup_Battle_Retire",
            "DialogCategory": "Battle",
            "VoiceClip": "Hasumi_(Sportswear)_Battle_Retire",
            "LocalizeJP": "そんな…",
            "LocalizeEN": "This can't be..."
        },
        {
            "CharacterId": 16011,
            "CharacterVoiceGroupId": 16011,
            "LocalizeCVGroup": "CVGroup_Battle_Shout_1",
            "DialogCategory": "Battle",
            "VoiceClip": "Hasumi_(Sportswear)_Battle_Shout_1",
            "LocalizeJP": "",
            "LocalizeEN": ""
        },
        {
            "CharacterId": 16011,
            "CharacterVoiceGroupId": 16011,
            "LocalizeCVGroup": "CVGroup_Battle_Shout_2",
            "DialogCategory": "Battle",
            "VoiceClip": "Hasumi_(Sportswear)_Battle_Shout_2",
            "LocalizeJP": "",
            "LocalizeEN": ""
        },
        {
            "CharacterId": 16011,
            "CharacterVoiceGroupId": 16011,
            "LocalizeCVGroup": "CVGroup_Battle_Shout_3",
            "DialogCategory": "Battle",
            "VoiceClip": "Hasumi_(Sportswear)_Battle_Shout_3",
            "LocalizeJP": "発射します。",
            "LocalizeEN": "Fire!"
        },
        {
            "CharacterId": 16011,
            "CharacterVoiceGroupId": 16011,
            "LocalizeCVGroup": "CVGroup_Battle_TacticalAction_1",
            "DialogCategory": "Battle",
            "VoiceClip": "Hasumi_(Sportswear)_Battle_TacticalAction_1",
            "LocalizeJP": "次へ行きましょう。",
            "LocalizeEN": "Moving to the next destination."
        },
        {
            "CharacterId": 16011,
            "CharacterVoiceGroupId": 16011,
            "LocalizeCVGroup": "CVGroup_Battle_Victory_1",
            "DialogCategory": "Battle",
            "VoiceClip": "Hasumi_(Sportswear)_Battle_Victory_1",
            "LocalizeJP": "素晴らしい試合でした。",
            "LocalizeEN": "It was a great match."
        },
        {
            "CharacterId": 16011,
            "CharacterVoiceGroupId": 16011,
            "LocalizeCVGroup": "CVGroup_Battle_Victory_2",
            "DialogCategory": "Battle",
            "VoiceClip": "Hasumi_(Sportswear)_Battle_Victory_2",
            "LocalizeJP": "これくらいは達成できないといけませんね。",
            "LocalizeEN": "We should at least be able to achieve this much."
        },
        {
            "CharacterId": 16011,
            "CharacterVoiceGroupId": 16011,
            "LocalizeCVGroup": "CVGroup_CommonSkill",
            "DialogCategory": "CommonSkill",
            "VoiceClip": "Hasumi_(Sportswear)_CommonSkill",
            "LocalizeJP": "集中します。",
            "LocalizeEN": "Focus."
        },
        {
            "CharacterId": 16011,
            "CharacterVoiceGroupId": 16011,
            "LocalizeCVGroup": "CVGroup_ExSkill_1",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Hasumi_(Sportswear)_ExSkill_1",
            "LocalizeJP": "美味しそう……。",
            "LocalizeEN": "Looks delicious."
        },
        {
            "CharacterId": 16011,
            "CharacterVoiceGroupId": 16011,
            "LocalizeCVGroup": "CVGroup_ExSkill_2",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Hasumi_(Sportswear)_ExSkill_2",
            "LocalizeJP": "うぅ……。",
            "LocalizeEN": ""
        },
        {
            "CharacterId": 16011,
            "CharacterVoiceGroupId": 16011,
            "LocalizeCVGroup": "CVGroup_ExSkill_3",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Hasumi_(Sportswear)_ExSkill_3",
            "LocalizeJP": "耐え難い誘惑です……。",
            "LocalizeEN": "The temptation is hard to resist."
        },
        {
            "CharacterId": 16011,
            "CharacterVoiceGroupId": 16011,
            "LocalizeCVGroup": "CVGroup_ExSkill_Level_1",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Hasumi_(Sportswear)_ExSkill_Level_1",
            "LocalizeJP": "集中、集中……！",
            "LocalizeEN": "Focus, focus!"
        },
        {
            "CharacterId": 16011,
            "CharacterVoiceGroupId": 16011,
            "LocalizeCVGroup": "CVGroup_ExSkill_Level_2",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Hasumi_(Sportswear)_ExSkill_Level_2",
            "LocalizeJP": "こんな時こそ深呼吸を……！",
            "LocalizeEN": "At times like this take a deep breath..."
        },
        {
            "CharacterId": 16011,
            "CharacterVoiceGroupId": 16011,
            "LocalizeCVGroup": "CVGroup_ExSkill_Level_3",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Hasumi_(Sportswear)_ExSkill_Level_3",
            "LocalizeJP": "しっかりしなさい、ハスミ！",
            "LocalizeEN": "Pull yourself together, Hasumi!"
        },
        {
            "CharacterId": 16011,
            "CharacterVoiceGroupId": 16011,
            "LocalizeCVGroup": "CVGroup_Growup_1",
            "DialogCategory": "Growup",
            "VoiceClip": "Hasumi_(Sportswear)_Growup_1",
            "LocalizeJP": "期待してくださった分、結果でお返しいたしますね。",
            "LocalizeEN": "I will turn your expectations into results."
        },
        {
            "CharacterId": 16011,
            "CharacterVoiceGroupId": 16011,
            "LocalizeCVGroup": "CVGroup_Growup_2",
            "DialogCategory": "Growup",
            "VoiceClip": "Hasumi_(Sportswear)_Growup_2",
            "LocalizeJP": "ありがとうございます。このご恩は、いつか必ず……。",
            "LocalizeEN": "Thank you. I will definitely repay you for this."
        },
        {
            "CharacterId": 16011,
            "CharacterVoiceGroupId": 16011,
            "LocalizeCVGroup": "CVGroup_Growup_3",
            "DialogCategory": "Growup",
            "VoiceClip": "Hasumi_(Sportswear)_Growup_3",
            "LocalizeJP": "これも試合のサポート……でしょうか？",
            "LocalizeEN": "Is this also to a means to support me for the match?"
        },
        {
            "CharacterId": 16011,
            "CharacterVoiceGroupId": 16011,
            "LocalizeCVGroup": "CVGroup_Growup_4",
            "DialogCategory": "Growup",
            "VoiceClip": "Hasumi_(Sportswear)_Growup_4",
            "LocalizeJP": "こんなに期待していただいたのですから、全力を尽くすしかないですね。",
            "LocalizeEN": "With this much expectation I have no choice but to do my best."
        },
        {
            "CharacterId": 16011,
            "CharacterVoiceGroupId": 16011,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_1",
            "DialogCategory": "Relationship",
            "VoiceClip": "Hasumi_(Sportswear)_Relationship_Up_1",
            "LocalizeJP": "お側にいてくださるだけでも心強いです。",
            "LocalizeEN": "Just having you by my side is reassuring."
        },
        {
            "CharacterId": 16011,
            "CharacterVoiceGroupId": 16011,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_2",
            "DialogCategory": "Relationship",
            "VoiceClip": "Hasumi_(Sportswear)_Relationship_Up_2",
            "LocalizeJP": "先生さえよろしければ、もう少し一緒に……。",
            "LocalizeEN": "Sensei, if you don't mind, let's stay together a little longer."
        },
        {
            "CharacterId": 16011,
            "CharacterVoiceGroupId": 16011,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_3",
            "DialogCategory": "Relationship",
            "VoiceClip": "Hasumi_(Sportswear)_Relationship_Up_3",
            "LocalizeJP": "たまには先生も……欲しいものをおっしゃってくださいね。",
            "LocalizeEN": "Please tell me what you want as well, once in a while."
        },
        {
            "CharacterId": 16011,
            "CharacterVoiceGroupId": 16011,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_4",
            "DialogCategory": "Relationship",
            "VoiceClip": "Hasumi_(Sportswear)_Relationship_Up_4",
            "LocalizeJP": "ありがとうございます……こんな風に励ましていただいたのは久しぶりです。",
            "LocalizeEN": "Thank you. It's been long since I was last encouraged this way."
        }
    ]
}
