{
    "DataList": [
        {
            "CharacterId": 26004,
            "CharacterVoiceGroupId": 26004,
            "LocalizeCVGroup": "CVGroup_Formation_In_1",
            "DialogCategory": "Formation",
            "VoiceClip": "Shimiko_Formation_In_1",
            "LocalizeJP": "仕方ないですね。行きます。",
            "LocalizeEN": "Alright, I'll go."
        },
        {
            "CharacterId": 26004,
            "CharacterVoiceGroupId": 26004,
            "LocalizeCVGroup": "CVGroup_Formation_In_2",
            "DialogCategory": "Formation",
            "VoiceClip": "Shimiko_Formation_In_2",
            "LocalizeJP": "私のことが欲しいのですか？",
            "LocalizeEN": "Do you want me?"
        },
        {
            "CharacterId": 26004,
            "CharacterVoiceGroupId": 26004,
            "LocalizeCVGroup": "CVGroup_Battle_In_1",
            "DialogCategory": "Battle",
            "VoiceClip": "Shimiko_Battle_In_1",
            "LocalizeJP": "支援はお任せください。",
            "LocalizeEN": "Please leave the support to me."
        },
        {
            "CharacterId": 26004,
            "CharacterVoiceGroupId": 26004,
            "LocalizeCVGroup": "CVGroup_Battle_In_2",
            "DialogCategory": "Battle",
            "VoiceClip": "Shimiko_Battle_In_2",
            "LocalizeJP": "なんだか少しドキドキしますね。",
            "LocalizeEN": "I'm somehow feeling a little excited."
        },
        {
            "CharacterId": 26004,
            "CharacterVoiceGroupId": 26004,
            "LocalizeCVGroup": "CVGroup_CommonSkill",
            "DialogCategory": "CommonSkill",
            "VoiceClip": "Shimiko_CommonSkill",
            "LocalizeJP": "知識は心の糧ですね。",
            "LocalizeEN": "Knowledge is food for the soul."
        },
        {
            "CharacterId": 26004,
            "CharacterVoiceGroupId": 26004,
            "LocalizeCVGroup": "CVGroup_ExSkill_1",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Shimiko_ExSkill_1",
            "LocalizeJP": "面白いです。",
            "LocalizeEN": "That's interesting."
        },
        {
            "CharacterId": 26004,
            "CharacterVoiceGroupId": 26004,
            "LocalizeCVGroup": "CVGroup_ExSkill_2",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Shimiko_ExSkill_2",
            "LocalizeJP": "感動しました。",
            "LocalizeEN": "I was moved."
        },
        {
            "CharacterId": 26004,
            "CharacterVoiceGroupId": 26004,
            "LocalizeCVGroup": "CVGroup_ExSkill_3",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Shimiko_ExSkill_3",
            "LocalizeJP": "これは愛。",
            "LocalizeEN": "This is love."
        },
        {
            "CharacterId": 26004,
            "CharacterVoiceGroupId": 26004,
            "LocalizeCVGroup": "CVGroup_ExSkill_Level_1",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Shimiko_ExSkill_Level_1",
            "LocalizeJP": "皆さんもっと読書してください。",
            "LocalizeEN": "Please read more, everyone."
        },
        {
            "CharacterId": 26004,
            "CharacterVoiceGroupId": 26004,
            "LocalizeCVGroup": "CVGroup_ExSkill_Level_2",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Shimiko_ExSkill_Level_2",
            "LocalizeJP": "私と一緒に読書しましょう。",
            "LocalizeEN": "Let's read together with me."
        },
        {
            "CharacterId": 26004,
            "CharacterVoiceGroupId": 26004,
            "LocalizeCVGroup": "CVGroup_ExSkill_Level_3",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Shimiko_ExSkill_Level_3",
            "LocalizeJP": "これもまた本の力です。",
            "LocalizeEN": "This is also the power of books."
        },
        {
            "CharacterId": 26004,
            "CharacterVoiceGroupId": 26004,
            "LocalizeCVGroup": "CVGroup_Growup_1",
            "DialogCategory": "Growup",
            "VoiceClip": "Shimiko_Growup_1",
            "LocalizeJP": "ありがとうございます。先生",
            "LocalizeEN": "Thank you, Sensei."
        },
        {
            "CharacterId": 26004,
            "CharacterVoiceGroupId": 26004,
            "LocalizeCVGroup": "CVGroup_Growup_2",
            "DialogCategory": "Growup",
            "VoiceClip": "Shimiko_Growup_2",
            "LocalizeJP": "本を読めば読むほど、強くなれます。",
            "LocalizeEN": "The more I read books, the stronger I become."
        },
        {
            "CharacterId": 26004,
            "CharacterVoiceGroupId": 26004,
            "LocalizeCVGroup": "CVGroup_Growup_3",
            "DialogCategory": "Growup",
            "VoiceClip": "Shimiko_Growup_3",
            "LocalizeJP": "本には世界中の知恵が盛り込まれています。",
            "LocalizeEN": "Books contain wisdom from around the world."
        },
        {
            "CharacterId": 26004,
            "CharacterVoiceGroupId": 26004,
            "LocalizeCVGroup": "CVGroup_Growup_4",
            "DialogCategory": "Growup",
            "VoiceClip": "Shimiko_Growup_4",
            "LocalizeJP": "また知識が増えましたね。",
            "LocalizeEN": "I have again gained more knowledge."
        },
        {
            "CharacterId": 26004,
            "CharacterVoiceGroupId": 26004,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_1",
            "DialogCategory": "Relationship",
            "VoiceClip": "Shimiko_Relationship_Up_1",
            "LocalizeJP": "愛がどんなものか知ってます。本でしっかり学びましたから。",
            "LocalizeEN": "I know what love is. I learned it properly from a book."
        },
        {
            "CharacterId": 26004,
            "CharacterVoiceGroupId": 26004,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_2",
            "DialogCategory": "Relationship",
            "VoiceClip": "Shimiko_Relationship_Up_2",
            "LocalizeJP": "まるでいい本を読んだ後のような、幸せな気持ちです。",
            "LocalizeEN": "I feel happy, just as I would after reading a good book."
        },
        {
            "CharacterId": 26004,
            "CharacterVoiceGroupId": 26004,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_3",
            "DialogCategory": "Relationship",
            "VoiceClip": "Shimiko_Relationship_Up_3",
            "LocalizeJP": "書物では学べなかったものがありました、例えば先生のこととか。",
            "LocalizeEN": "There were things that books couldn't teach me, such as things about Sensei."
        },
        {
            "CharacterId": 26004,
            "CharacterVoiceGroupId": 26004,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_4",
            "DialogCategory": "Relationship",
            "VoiceClip": "Shimiko_Relationship_Up_4",
            "LocalizeJP": "知らなかったことを知ることができるのは、とても嬉しいことです。先生は私についてどれぐらい知りたいですか。",
            "LocalizeEN": "It's a pleasure to learn things that one didn't know. Sensei, how much do you want to know about me?"
        }
    ]
}
