        {
            "CharacterId": 16003,
            "CharacterVoiceGroupId": 16003,
            "LocalizeCVGroup": "CVGroup_Battle_Victory_1",
            "DialogCategory": "Battle",
            "VoiceClip": "Suzumi_Battle_Victory_1",
            "LocalizeJP": "パトロールを終了します。",
            "LocalizeEN": "End the patrol."
        },
        {
            "CharacterId": 16003,
            "CharacterVoiceGroupId": 16003,
            "LocalizeCVGroup": "CVGroup_Battle_Victory_2",
            "DialogCategory": "Battle",
            "VoiceClip": "Suzumi_Battle_Victory_2",
            "LocalizeJP": "簡単な任務でした。",
            "LocalizeEN": "It was an easy task."
        },
        {
            "CharacterId": 16003,
            "CharacterVoiceGroupId": 16003,
            "LocalizeCVGroup": "CVGroup_CommonSkill",
            "DialogCategory": "CommonSkill",
            "VoiceClip": "Suzumi_CommonSkill",
            "LocalizeJP": "逃しません。",
            "LocalizeEN": "I won't miss it."
        },
        {
            "CharacterId": 16003,
            "CharacterVoiceGroupId": 16003,
            "LocalizeCVGroup": "CVGroup_ExSkill_1",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Suzumi_ExSkill_1",
            "LocalizeJP": "腰を抜かさせてあげましょう。",
            "LocalizeEN": "Let me get off my knees."
        },
        {
            "CharacterId": 16003,
            "CharacterVoiceGroupId": 16003,
            "LocalizeCVGroup": "CVGroup_ExSkill_2",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Suzumi_ExSkill_2",
            "LocalizeJP": "天罰の光を",
            "LocalizeEN": "light of retribution"
        },
        {
            "CharacterId": 16003,
            "CharacterVoiceGroupId": 16003,
            "LocalizeCVGroup": "CVGroup_ExSkill_3",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Suzumi_ExSkill_3",
            "LocalizeJP": "これは痛いですよ。",
            "LocalizeEN": "This hurts."
        },
        {
            "CharacterId": 16003,
            "CharacterVoiceGroupId": 16003,
            "LocalizeCVGroup": "CVGroup_ExSkill_Level_1",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Suzumi_ExSkill_Level_1",
            "LocalizeJP": "今です！",
            "LocalizeEN": "now"
        },
        {
            "CharacterId": 16003,
            "CharacterVoiceGroupId": 16003,
            "LocalizeCVGroup": "CVGroup_ExSkill_Level_2",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Suzumi_ExSkill_Level_2",
            "LocalizeJP": "閃光弾、投擲。",
            "LocalizeEN": "flash shot throw"
        },
        {
            "CharacterId": 16003,
            "CharacterVoiceGroupId": 16003,
            "LocalizeCVGroup": "CVGroup_ExSkill_Level_3",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Suzumi_ExSkill_Level_3",
            "LocalizeJP": "前方へ閃光弾を。",
            "LocalizeEN": "Fire a flashbang in front."
        },
        {
            "CharacterId": 16003,
            "CharacterVoiceGroupId": 16003,
            "LocalizeCVGroup": "CVGroup_Growup_1",
            "DialogCategory": "Growup",
            "VoiceClip": "Suzumi_Growup_1",
            "LocalizeJP": "これなら…、パトロールの役に立てそうです。",
            "LocalizeEN": "This would be useful for patrols."
        },
        {
            "CharacterId": 16003,
            "CharacterVoiceGroupId": 16003,
            "LocalizeCVGroup": "CVGroup_Growup_2",
            "DialogCategory": "Growup",
            "VoiceClip": "Suzumi_Growup_2",
            "LocalizeJP": "ふむ、悪くないですね。",
            "LocalizeEN": "Not bad."
        },
        {
            "CharacterId": 16003,
            "CharacterVoiceGroupId": 16003,
            "LocalizeCVGroup": "CVGroup_Growup_3",
            "DialogCategory": "Growup",
            "VoiceClip": "Suzumi_Growup_3",
            "LocalizeJP": "いい感じです。",
            "LocalizeEN": "Good vibes."
        },
        {
            "CharacterId": 16003,
            "CharacterVoiceGroupId": 16003,
            "LocalizeCVGroup": "CVGroup_Growup_4",
            "DialogCategory": "Growup",
            "VoiceClip": "Suzumi_Growup_4",
            "LocalizeJP": "これからもよろしくお願いします。",
            "LocalizeEN": "I look forward to working with you."
        },
        {
            "CharacterId": 16003,
            "CharacterVoiceGroupId": 16003,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_1",
            "DialogCategory": "Relationship",
            "VoiceClip": "Suzumi_Relationship_Up_1",
            "LocalizeJP": "いつも頼りになります。ありがとうございます、先生。",
            "LocalizeEN": "Thank you very much, teacher"
        },
        {
            "CharacterId": 16003,
            "CharacterVoiceGroupId": 16003,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_2",
            "DialogCategory": "Relationship",
            "VoiceClip": "Suzumi_Relationship_Up_2",
            "LocalizeJP": "先生と一緒のパトロールは、いつもよりも短く感じます。何ででしょうか。",
            "LocalizeEN": "The patrol with the teacher feels shorter than usual. Why?"
        },
        {
            "CharacterId": 16003,
            "CharacterVoiceGroupId": 16003,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_3",
            "DialogCategory": "Relationship",
            "VoiceClip": "Suzumi_Relationship_Up_3",
            "LocalizeJP": "こういうのは幸せと言うんでしょうか。パトロールを終えた時の高揚感とはまた違いますね。",
            "LocalizeEN": "Is this what you call happiness?"
        },
        {
            "CharacterId": 16003,
            "CharacterVoiceGroupId": 16003,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_4",
            "DialogCategory": "Relationship",
            "VoiceClip": "Suzumi_Relationship_Up_4",
            "LocalizeJP": "先生、これからもずっと私と一緒にいてほしいです。",
            "LocalizeEN": "Sensei, I want you to stay with me forever."
        }
    ]
}
