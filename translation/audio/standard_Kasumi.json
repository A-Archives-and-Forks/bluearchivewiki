            "LocalizeEN": ""
        },
        {
            "CharacterId": 10078,
            "DialogCategory": "ExSkill",
            "VoiceClip": "Kasumi_ExSkill_Level_3",
            "LocalizeJP": "いくぞいくぞ、と！ハハハハ！",
