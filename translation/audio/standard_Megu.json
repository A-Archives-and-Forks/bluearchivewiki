        {
            "CharacterId": 10060,
            "CharacterVoiceGroupId": 10060,
            "LocalizeCVGroup": "CVGroup_ExSkill_1",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Megu_ExSkill_1",
