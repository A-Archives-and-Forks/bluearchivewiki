{
    "DataList": [
        {
            "CharacterId": 20031,
            "CharacterVoiceGroupId": 20031,
            "LocalizeCVGroup": "CVGroup_CommonSkill",
            "DialogCategory": "CommonSkill",
            "VoiceClip": "Shigure_(Hot_Spring)_CommonSkill",
            "LocalizeJP": "温泉まんじゅうでもどう？",
            "LocalizeEN": "How about some Onsen Manju?"
        },
        {
            "CharacterId": 20031,
            "CharacterVoiceGroupId": 20031,
            "LocalizeCVGroup": "CVGroup_ExSkill_1",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Shigure_(Hot_Spring)_ExSkill_1",
            "LocalizeJP": "はい、先生。今日の祝杯を挙げるよ。",
            "LocalizeEN": "Yes Sensei. Let's have a toast today."
        },
        {
            "CharacterId": 20031,
            "CharacterVoiceGroupId": 20031,
            "LocalizeCVGroup": "CVGroup_ExSkill_2",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Shigure_(Hot_Spring)_ExSkill_2",
            "LocalizeJP": "かんぱ～い！",
            "LocalizeEN": "Cheers!"
        },
        {
            "CharacterId": 20031,
            "CharacterVoiceGroupId": 20031,
            "LocalizeCVGroup": "CVGroup_ExSkill_3",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Shigure_(Hot_Spring)_ExSkill_3",
            "LocalizeJP": "ナ・ズダローヴィエ！",
            "LocalizeEN": "Enjoy! ([[wikt:на_здоровье|На здоровье]]!)"
        },
        {
            "CharacterId": 20031,
            "CharacterVoiceGroupId": 20031,
            "LocalizeCVGroup": "CVGroup_ExSkill_Level_1",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Shigure_(Hot_Spring)_ExSkill_Level_1",
            "LocalizeJP": "227号温泉郷の特設ドリンクバー、営業開始だよっ。",
            "LocalizeEN": "No. 227 Hot Spring Resort's drink bar is open for business."
        },
        {
            "CharacterId": 20031,
            "CharacterVoiceGroupId": 20031,
            "LocalizeCVGroup": "CVGroup_ExSkill_Level_2",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Shigure_(Hot_Spring)_ExSkill_Level_2",
            "LocalizeJP": "お酒じゃないから、みんな安心して飲んでね。",
            "LocalizeEN": "This is not alcohol. Relax and drink, everyone."
        },
        {
            "CharacterId": 20031,
            "CharacterVoiceGroupId": 20031,
            "LocalizeCVGroup": "CVGroup_ExSkill_Level_3",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Shigure_(Hot_Spring)_ExSkill_Level_3",
            "LocalizeJP": "さあさ、一杯どうぞ。",
            "LocalizeEN": "Now, have a drink."
        },
        {
            "CharacterId": 20031,
            "CharacterVoiceGroupId": 20031,
            "LocalizeCVGroup": "CVGroup_Growup_1",
            "DialogCategory": "Growup",
            "VoiceClip": "Shigure_(Hot_Spring)_Growup_1",
            "LocalizeJP": "おっ、これは印象的な発酵……いや、成長だね。",
            "LocalizeEN": "Ah, this is an impressive fermentation... no, an impressive growth."
        },
        {
            "CharacterId": 20031,
            "CharacterVoiceGroupId": 20031,
            "LocalizeCVGroup": "CVGroup_Growup_2",
            "DialogCategory": "Growup",
            "VoiceClip": "Shigure_(Hot_Spring)_Growup_2",
            "LocalizeJP": "これもドリンク作りに使えるかな？\n……ふふ、冗談だって。",
            "LocalizeEN": "Can this also be used to make alcohol? Just kidding."
        },
        {
            "CharacterId": 20031,
            "CharacterVoiceGroupId": 20031,
            "LocalizeCVGroup": "CVGroup_Growup_3",
            "DialogCategory": "Growup",
            "VoiceClip": "Shigure_(Hot_Spring)_Growup_3",
            "LocalizeJP": "どこにいても、たゆまぬ努力は大事だよね。",
            "LocalizeEN": "Wherever I am, working hard is important."
        },
        {
            "CharacterId": 20031,
            "CharacterVoiceGroupId": 20031,
            "LocalizeCVGroup": "CVGroup_Growup_4",
            "DialogCategory": "Growup",
            "VoiceClip": "Shigure_(Hot_Spring)_Growup_4",
            "LocalizeJP": "いつも手伝ってくれてありがとね、先生。",
            "LocalizeEN": "Thank you for always helping me out, Sensei."
        },
        {
            "CharacterId": 20031,
            "CharacterVoiceGroupId": 20031,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_1",
            "DialogCategory": "Relationship",
            "VoiceClip": "Shigure_(Hot_Spring)_Relationship_Up_1",
            "LocalizeJP": "先生、ちょ～っとあっちの方でおしゃべりしない？",
            "LocalizeEN": "Sensei, do you want to chat with me out there for a bit?"
        },
        {
            "CharacterId": 20031,
            "CharacterVoiceGroupId": 20031,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_2",
            "DialogCategory": "Relationship",
            "VoiceClip": "Shigure_(Hot_Spring)_Relationship_Up_2",
            "LocalizeJP": "……扉を開けとくからさ。\n暇なら遊びにきていいからね？",
            "LocalizeEN": "I'll leave the door open, so feel free to come hang out when you're free."
        },
        {
            "CharacterId": 20031,
            "CharacterVoiceGroupId": 20031,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_3",
            "DialogCategory": "Relationship",
            "VoiceClip": "Shigure_(Hot_Spring)_Relationship_Up_3",
            "LocalizeJP": "先生、いい表情してる。\n何でみんなが夢中になるのか、わかる気がするよ。",
            "LocalizeEN": "Sensei, you have a great facial expression. I understand why everyone is so into you."
        },
        {
            "CharacterId": 20031,
            "CharacterVoiceGroupId": 20031,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_4",
            "DialogCategory": "Relationship",
            "VoiceClip": "Shigure_(Hot_Spring)_Relationship_Up_4",
            "LocalizeJP": "いつか本当にお酒を飲めるようになったら……\n最初の一杯は先生と一緒がいいな。",
            "LocalizeEN": "When I'm able to drink alcohol, it'll be great to have my first drink together with Sensei."
        }
    ]
}
