{
    "DataList": [
        {
            "CharacterId": 20029,
            "CharacterVoiceGroupId": 20029,
            "LocalizeCVGroup": "CVGroup_Formation_In_1",
            "DialogCategory": "Formation",
            "VoiceClip": "Mimori_(Swimsuit)_Formation_In_1",
            "LocalizeJP": "はい、先生。\nお待ちしておりました。",
            "LocalizeEN": "All ready, Sensei! Thank you for waiting."
        },
        {
            "CharacterId": 20029,
            "CharacterVoiceGroupId": 20029,
            "LocalizeCVGroup": "CVGroup_Formation_In_2",
            "DialogCategory": "Formation",
            "VoiceClip": "Mimori_(Swimsuit)_Formation_In_2",
            "LocalizeJP": "なんだか、楽しみですね……\nどんな夏が、私たちを待っているのでしょう？",
            "LocalizeEN": "I wonder... what kind of summer awaits us?"
        },
        {
            "CharacterId": 20029,
            "CharacterVoiceGroupId": 20029,
            "LocalizeCVGroup": "CVGroup_CommonSkill",
            "DialogCategory": "CommonSkill",
            "VoiceClip": "Mimori_(Swimsuit)_CommonSkill",
            "LocalizeJP": "たくさん食べていってくださいね。",
            "LocalizeEN": "Make sure to eat your fill!"
        },
        {
            "CharacterId": 20029,
            "CharacterVoiceGroupId": 20029,
            "LocalizeCVGroup": "CVGroup_ExSkill_1",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Mimori_(Swimsuit)_ExSkill_1",
            "LocalizeJP": "よいしょ、よいしょ……\n先生、こちらですよ。",
            "LocalizeEN": "...And there! Sensei! Over here!"
        },
        {
            "CharacterId": 20029,
            "CharacterVoiceGroupId": 20029,
            "LocalizeCVGroup": "CVGroup_ExSkill_2",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Mimori_(Swimsuit)_ExSkill_2",
            "LocalizeJP": "さて、準備は万全……\nようこそおいでくださりました、先生。",
            "LocalizeEN": "Alright, preparations are done. Everything's ready for you, Sensei!"
        },
        {
            "CharacterId": 20029,
            "CharacterVoiceGroupId": 20029,
            "LocalizeCVGroup": "CVGroup_ExSkill_3",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Mimori_(Swimsuit)_ExSkill_3",
            "LocalizeJP": "素敵な海ですね……\nええ、お待ちしておりました。",
            "LocalizeEN": "What a wonderful ocean view... Thank you for your patience!"
        },
        {
            "CharacterId": 20029,
            "CharacterVoiceGroupId": 20029,
            "LocalizeCVGroup": "CVGroup_ExSkill_Level_1",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Mimori_(Swimsuit)_ExSkill_Level_1",
            "LocalizeJP": "休憩は大事ですから。",
            "LocalizeEN": "Proper rest is important, too!"
        },
        {
            "CharacterId": 20029,
            "CharacterVoiceGroupId": 20029,
            "LocalizeCVGroup": "CVGroup_ExSkill_Level_2",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Mimori_(Swimsuit)_ExSkill_Level_2",
            "LocalizeJP": "紫外線には気をつけてくださいね？",
            "LocalizeEN": "Be wary of ultraviolet rays."
        },
        {
            "CharacterId": 20029,
            "CharacterVoiceGroupId": 20029,
            "LocalizeCVGroup": "CVGroup_ExSkill_Level_3",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Mimori_(Swimsuit)_ExSkill_Level_3",
            "LocalizeJP": "皆さん、よろしければどうぞ。",
            "LocalizeEN": "Everyone, feel free to use as necessary."
        },
        {
            "CharacterId": 20029,
            "CharacterVoiceGroupId": 20029,
            "LocalizeCVGroup": "CVGroup_Growup_1",
            "DialogCategory": "Growup",
            "VoiceClip": "Mimori_(Swimsuit)_Growup_1",
            "LocalizeJP": "夏の修行、ですね……ふふっ。嬉しいです。",
            "LocalizeEN": "Summer training? Hehe, that sounds delightful."
        },
        {
            "CharacterId": 20029,
            "CharacterVoiceGroupId": 20029,
            "LocalizeCVGroup": "CVGroup_Growup_2",
            "DialogCategory": "Growup",
            "VoiceClip": "Mimori_(Swimsuit)_Growup_2",
            "LocalizeJP": "これは……修行の成果でしょうか。\nそれとも先生のおかげ、でしょうか……？",
            "LocalizeEN": "Is this the result of my training? Or perhaps, is it because of Sensei..."
        },
        {
            "CharacterId": 20029,
            "CharacterVoiceGroupId": 20029,
            "LocalizeCVGroup": "CVGroup_Growup_3",
            "DialogCategory": "Growup",
            "VoiceClip": "Mimori_(Swimsuit)_Growup_3",
            "LocalizeJP": "先生が見てくださったおかげで……\n私、こんなに成長できました。ありがとうございます。",
            "LocalizeEN": "Under your watchful guidance, I was able to grow so much... Thank you."
        },
        {
            "CharacterId": 20029,
            "CharacterVoiceGroupId": 20029,
            "LocalizeCVGroup": "CVGroup_Growup_4",
            "DialogCategory": "Growup",
            "VoiceClip": "Mimori_(Swimsuit)_Growup_4",
            "LocalizeJP": "何だか……とても楽しみです。\n私、今胸がときめいて……幸せです。",
            "LocalizeEN": "I've... had a lot of fun! My chest is beating, but... I'm happy!"
        },
        {
            "CharacterId": 20029,
            "CharacterVoiceGroupId": 20029,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_1",
            "DialogCategory": "Relationship",
            "VoiceClip": "Mimori_(Swimsuit)_Relationship_Up_1",
            "LocalizeJP": "先生と一緒なら……\n素敵な事が起こりそうな予感がします。",
            "LocalizeEN": "When I'm with you, Sensei, I always feel that wonderful things are just around the corner."
        },
        {
            "CharacterId": 20029,
            "CharacterVoiceGroupId": 20029,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_2",
            "DialogCategory": "Relationship",
            "VoiceClip": "Mimori_(Swimsuit)_Relationship_Up_2",
            "LocalizeJP": "できるなら、先生の半歩後ろを……\nはうぅ、口に出そうとすると恥ずかしいです……",
            "LocalizeEN": "If it's alright with you, could I... walk a half step behind...\nAhh! When I say it out loud like that, it sounds so embarrassing...!"
        },
        {
            "CharacterId": 20029,
            "CharacterVoiceGroupId": 20029,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_3",
            "DialogCategory": "Relationship",
            "VoiceClip": "Mimori_(Swimsuit)_Relationship_Up_3",
            "LocalizeJP": "……幸せです。\n先生とご一緒するこの時間……",
            "LocalizeEN": "Whenever I'm with you, Sensei... I am truly happy."
        },
        {
            "CharacterId": 20029,
            "CharacterVoiceGroupId": 20029,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_4",
            "DialogCategory": "Relationship",
            "VoiceClip": "Mimori_(Swimsuit)_Relationship_Up_4",
            "LocalizeJP": "あの、先生……\nいいえ、なんでもありません。\nその……もう少し待っていてください。……もう少し、だけ。",
            "LocalizeEN": "Um, Sensei... ah, no, never mind... If you could just wait a little longer. Just... a little bit longer..."
        }
    ]
}
