{
    "DataList": [
        {
            "CharacterId": 10011,
            "CharacterVoiceGroupId": 10011,
            "LocalizeCVGroup": "CVGroup_Formation_In_1",
            "DialogCategory": "Formation",
            "VoiceClip": "Shun_Formation_In_1",
            "LocalizeJP": "うふふ、素晴らしい選択ですね。",
            "LocalizeEN": "Ufufu, what a wonderful choice."
        },
        {
            "CharacterId": 10011,
            "CharacterVoiceGroupId": 10011,
            "LocalizeCVGroup": "CVGroup_Formation_In_2",
            "DialogCategory": "Formation",
            "VoiceClip": "Shun_Formation_In_2",
            "LocalizeJP": "お姉さんにお任せください。",
            "LocalizeEN": "Leave it to me."
        },
        {
            "CharacterId": 10011,
            "CharacterVoiceGroupId": 10011,
            "LocalizeCVGroup": "CVGroup_Formation_Select",
            "DialogCategory": "Formation",
            "VoiceClip": "Shun_Formation_Select",
            "LocalizeJP": "あら、何か問題が…？",
            "LocalizeEN": "Oh, is there a problem...?"
        },
        {
            "CharacterId": 10011,
            "CharacterVoiceGroupId": 10011,
            "LocalizeCVGroup": "CVGroup_Tactic_Defeat_1",
            "DialogCategory": "Tactic",
            "VoiceClip": "Shun_Tactic_Defeat_1",
            "LocalizeJP": "み、みんな、怪我はなかった？",
            "LocalizeEN": "Is everyone alright? Are there any injuries?"
        },
        {
            "CharacterId": 10011,
            "CharacterVoiceGroupId": 10011,
            "LocalizeCVGroup": "CVGroup_Tactic_Defeat_2",
            "DialogCategory": "Tactic",
            "VoiceClip": "Shun_Tactic_Defeat_2",
            "LocalizeJP": "ごめんなさい、私の不注意でした。",
            "LocalizeEN": "I'm sorry, it was because of my carelessness."
        },
        {
            "CharacterId": 10011,
            "CharacterVoiceGroupId": 10011,
            "LocalizeCVGroup": "CVGroup_Tactic_In_1",
            "DialogCategory": "Tactic",
            "VoiceClip": "Shun_Tactic_In_1",
            "LocalizeJP": "言うこと聞かない悪い子はどこかな～。",
            "LocalizeEN": "Where is the naughty child who doesn't listen?"
        },
        {
            "CharacterId": 10011,
            "CharacterVoiceGroupId": 10011,
            "LocalizeCVGroup": "CVGroup_Tactic_In_2",
            "DialogCategory": "Tactic",
            "VoiceClip": "Shun_Tactic_In_2",
            "LocalizeJP": "さあ、かくれんぼスタートね。",
            "LocalizeEN": "Alright, let's start playing hide-and-seek."
        },
        {
            "CharacterId": 10011,
            "CharacterVoiceGroupId": 10011,
            "LocalizeCVGroup": "CVGroup_Tactic_Victory_1",
            "DialogCategory": "Tactic",
            "VoiceClip": "Shun_Tactic_Victory_1",
            "LocalizeJP": "えらいえらい。みんなよくできました。",
            "LocalizeEN": "Well done, everyone. You all did well."
        },
        {
            "CharacterId": 10011,
            "CharacterVoiceGroupId": 10011,
            "LocalizeCVGroup": "CVGroup_Tactic_Victory_2",
            "DialogCategory": "Tactic",
            "VoiceClip": "Shun_Tactic_Victory_2",
            "LocalizeJP": "よくできました、みんな。お家に帰ったら復習するのを忘れないでね。",
            "LocalizeEN": "Well done, everyone. Don't forget to review when you get home."
        },
        {
            "CharacterId": 10011,
            "CharacterVoiceGroupId": 10011,
            "LocalizeCVGroup": "CVGroup_Battle_Buffed_1",
            "DialogCategory": "Battle",
            "VoiceClip": "Shun_Battle_Buffed_1",
            "LocalizeJP": "お手伝いありがとうございます。",
            "LocalizeEN": "Thank you for your help."
        },
        {
            "CharacterId": 10011,
            "CharacterVoiceGroupId": 10011,
            "LocalizeCVGroup": "CVGroup_Battle_BuffSelf_1",
            "DialogCategory": "Battle",
            "VoiceClip": "Shun_Battle_BuffSelf_1",
            "LocalizeJP": "さあ、ちょっとだけ本気で行きましょうか。",
            "LocalizeEN": "Let's get a little serious."
        },
        {
            "CharacterId": 10011,
            "CharacterVoiceGroupId": 10011,
            "LocalizeCVGroup": "CVGroup_Battle_Covered_1",
            "DialogCategory": "Battle",
            "VoiceClip": "Shun_Battle_Covered_1",
            "LocalizeJP": "隠れます。",
            "LocalizeEN": "Hiding."
        },
        {
            "CharacterId": 10011,
            "CharacterVoiceGroupId": 10011,
            "LocalizeCVGroup": "CVGroup_Battle_Damage_1",
            "DialogCategory": "Battle",
            "VoiceClip": "Shun_Battle_Damage_1",
            "LocalizeJP": "あら。",
            "LocalizeEN": ""
        },
        {
            "CharacterId": 10011,
            "CharacterVoiceGroupId": 10011,
            "LocalizeCVGroup": "CVGroup_Battle_Damage_2",
            "DialogCategory": "Battle",
            "VoiceClip": "Shun_Battle_Damage_2",
            "LocalizeJP": "",
            "LocalizeEN": ""
        },
        {
            "CharacterId": 10011,
            "CharacterVoiceGroupId": 10011,
            "LocalizeCVGroup": "CVGroup_Battle_Damage_3",
            "DialogCategory": "Battle",
            "VoiceClip": "Shun_Battle_Damage_3",
            "LocalizeJP": "",
            "LocalizeEN": ""
        },
        {
            "CharacterId": 10011,
            "CharacterVoiceGroupId": 10011,
            "LocalizeCVGroup": "CVGroup_Battle_Defense_1",
            "DialogCategory": "Battle",
            "VoiceClip": "Shun_Battle_Defense_1",
            "LocalizeJP": "フフフ、そんないたずらは通用しませんよ。",
            "LocalizeEN": "Fufufu, such a trick won't work on me."
        },
        {
            "CharacterId": 10011,
            "CharacterVoiceGroupId": 10011,
            "LocalizeCVGroup": "CVGroup_Battle_In_1",
            "DialogCategory": "Battle",
            "VoiceClip": "Shun_Battle_In_1",
            "LocalizeJP": "さあ、みんなでお散歩に行きましょうか。",
            "LocalizeEN": "Now, let's go for a walk, everyone."
        },
        {
            "CharacterId": 10011,
            "CharacterVoiceGroupId": 10011,
            "LocalizeCVGroup": "CVGroup_Battle_In_2",
            "DialogCategory": "Battle",
            "VoiceClip": "Shun_Battle_In_2",
            "LocalizeJP": "みんな、道に迷わないように気をつけてね。",
            "LocalizeEN": "Everyone, be careful not to get lost."
        },
        {
            "CharacterId": 10011,
            "CharacterVoiceGroupId": 10011,
            "LocalizeCVGroup": "CVGroup_Battle_Move_1",
            "DialogCategory": "Battle",
            "VoiceClip": "Shun_Battle_Move_1",
            "LocalizeJP": "みんなー、こっちよー。",
            "LocalizeEN": "Everyone, over here!"
        },
        {
            "CharacterId": 10011,
            "CharacterVoiceGroupId": 10011,
            "LocalizeCVGroup": "CVGroup_Battle_Move_2",
            "DialogCategory": "Battle",
            "VoiceClip": "Shun_Battle_Move_2",
            "LocalizeJP": "道に迷わないように気をつけてね。",
            "LocalizeEN": "Be careful not to get lost on the way."
        },
        {
            "CharacterId": 10011,
            "CharacterVoiceGroupId": 10011,
            "LocalizeCVGroup": "CVGroup_Battle_Recovery_1",
            "DialogCategory": "Battle",
            "VoiceClip": "Shun_Battle_Recovery_1",
            "LocalizeJP": "あらありがとう。",
            "LocalizeEN": "Oh, thank you!"
        },
        {
            "CharacterId": 10011,
            "CharacterVoiceGroupId": 10011,
            "LocalizeCVGroup": "CVGroup_Battle_Retire",
            "DialogCategory": "Battle",
            "VoiceClip": "Shun_Battle_Retire",
            "LocalizeJP": "これ以上…、ごめんなさい。",
            "LocalizeEN": "That's it... I'm sorry."
        },
        {
            "CharacterId": 10011,
            "CharacterVoiceGroupId": 10011,
            "LocalizeCVGroup": "CVGroup_Battle_Shout_1",
            "DialogCategory": "Battle",
            "VoiceClip": "Shun_Battle_Shout_1",
            "LocalizeJP": "行きまーす。",
            "LocalizeEN": "Here I go."
        },
        {
            "CharacterId": 10011,
            "CharacterVoiceGroupId": 10011,
            "LocalizeCVGroup": "CVGroup_Battle_Shout_2",
            "DialogCategory": "Battle",
            "VoiceClip": "Shun_Battle_Shout_2",
            "LocalizeJP": "せーの。",
            "LocalizeEN": "One, two, three!"
        },
        {
            "CharacterId": 10011,
            "CharacterVoiceGroupId": 10011,
            "LocalizeCVGroup": "CVGroup_Battle_Shout_3",
            "DialogCategory": "Battle",
            "VoiceClip": "Shun_Battle_Shout_3",
            "LocalizeJP": "覚悟してね。",
            "LocalizeEN": "Be prepared."
        },
        {
            "CharacterId": 10011,
            "CharacterVoiceGroupId": 10011,
            "LocalizeCVGroup": "CVGroup_Battle_TacticalAction_1",
            "DialogCategory": "Battle",
            "VoiceClip": "Shun_Battle_TacticalAction_1",
            "LocalizeJP": "場所を移動します。",
            "LocalizeEN": "Changing my location."
        },
        {
            "CharacterId": 10011,
            "CharacterVoiceGroupId": 10011,
            "LocalizeCVGroup": "CVGroup_Battle_Victory_1",
            "DialogCategory": "Battle",
            "VoiceClip": "Shun_Battle_Victory_1",
            "LocalizeJP": "みんなご苦労様でした。",
            "LocalizeEN": "Thank you for your hard work, everyone."
        },
        {
            "CharacterId": 10011,
            "CharacterVoiceGroupId": 10011,
            "LocalizeCVGroup": "CVGroup_Battle_Victory_2",
            "DialogCategory": "Battle",
            "VoiceClip": "Shun_Battle_Victory_2",
            "LocalizeJP": "次からは仲良くするんですよ。",
            "LocalizeEN": "Let's get along from now on."
        },
        {
            "CharacterId": 10011,
            "CharacterVoiceGroupId": 10011,
            "LocalizeCVGroup": "CVGroup_CommonSkill",
            "DialogCategory": "CommonSkill",
            "VoiceClip": "Shun_CommonSkill",
            "LocalizeJP": "{{wikt|よいしょ}}ー！",
            "LocalizeEN": "Heave-ho!"
        },
        {
            "CharacterId": 10011,
            "CharacterVoiceGroupId": 10011,
            "LocalizeCVGroup": "CVGroup_ExSkill_1",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Shun_ExSkill_1",
            "LocalizeJP": "悪い子発見。",
            "LocalizeEN": "I found a bad boy."
        },
        {
            "CharacterId": 10011,
            "CharacterVoiceGroupId": 10011,
            "LocalizeCVGroup": "CVGroup_ExSkill_2",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Shun_ExSkill_2",
            "LocalizeJP": "ウフ、お姉さんがやるのをよく見ててね。",
            "LocalizeEN": "Carefully watch how your sister does it, okay?"
        },
        {
            "CharacterId": 10011,
            "CharacterVoiceGroupId": 10011,
            "LocalizeCVGroup": "CVGroup_ExSkill_3",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Shun_ExSkill_3",
            "LocalizeJP": "これで終わらせてしまいましょう。",
            "LocalizeEN": "Let's end the battle with this."
        },
        {
            "CharacterId": 10011,
            "CharacterVoiceGroupId": 10011,
            "LocalizeCVGroup": "CVGroup_ExSkill_Level_1",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Shun_ExSkill_Level_1",
            "LocalizeJP": "め、ですよ。",
            "LocalizeEN": "That's no good."
        },
        {
            "CharacterId": 10011,
            "CharacterVoiceGroupId": 10011,
            "LocalizeCVGroup": "CVGroup_ExSkill_Level_2",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Shun_ExSkill_Level_2",
            "LocalizeJP": "",
            "LocalizeEN": "Bang."
        },
        {
            "CharacterId": 10011,
            "CharacterVoiceGroupId": 10011,
            "LocalizeCVGroup": "CVGroup_ExSkill_Level_3",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Shun_ExSkill_Level_3",
            "LocalizeJP": "",
            "LocalizeEN": "Bang."
        },
        {
            "CharacterId": 10011,
            "CharacterVoiceGroupId": 10011,
            "LocalizeCVGroup": "CVGroup_Growup_1",
            "DialogCategory": "Growup",
            "VoiceClip": "Shun_Growup_1",
            "LocalizeJP": "成長はいつも気持ちのいいことですね。",
            "LocalizeEN": "Growth is always a pleasant thing."
        },
        {
            "CharacterId": 10011,
            "CharacterVoiceGroupId": 10011,
            "LocalizeCVGroup": "CVGroup_Growup_2",
            "DialogCategory": "Growup",
            "VoiceClip": "Shun_Growup_2",
            "LocalizeJP": "体の隅々にまで力が染み渡って…、気分が高揚しますね。",
            "LocalizeEN": "I can feel the power coursing through every corner of my body... It's exhilarating."
        },
        {
            "CharacterId": 10011,
            "CharacterVoiceGroupId": 10011,
            "LocalizeCVGroup": "CVGroup_Growup_3",
            "DialogCategory": "Growup",
            "VoiceClip": "Shun_Growup_3",
            "LocalizeJP": "心も体も丈夫、になりました。",
            "LocalizeEN": "My mind and body have become strong."
        },
        {
            "CharacterId": 10011,
            "CharacterVoiceGroupId": 10011,
            "LocalizeCVGroup": "CVGroup_Growup_4",
            "DialogCategory": "Growup",
            "VoiceClip": "Shun_Growup_4",
            "LocalizeJP": "子供達に教えるためには、 私が率先して成長しないといけないでしょう。",
            "LocalizeEN": "To teach the children, I must take the lead and grow myself."
        },
        {
            "CharacterId": 10011,
            "CharacterVoiceGroupId": 10011,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_1",
            "DialogCategory": "Relationship",
            "VoiceClip": "Shun_Relationship_Up_1",
            "LocalizeJP": "先生とご一緒していると、私もたまには子供みたいに甘えたくなります。",
            "LocalizeEN": "When I am with Sensei, I sometimes want to be spoiled like a child."
        },
        {
            "CharacterId": 10011,
            "CharacterVoiceGroupId": 10011,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_2",
            "DialogCategory": "Relationship",
            "VoiceClip": "Shun_Relationship_Up_2",
            "LocalizeJP": "先生は本当に特別な方ですね、生徒たちにとっても私にとっても。",
            "LocalizeEN": "Sensei is truly a special person, for the students and for me."
        },
        {
            "CharacterId": 10011,
            "CharacterVoiceGroupId": 10011,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_3",
            "DialogCategory": "Relationship",
            "VoiceClip": "Shun_Relationship_Up_3",
            "LocalizeJP": "先生との授業、楽しみにしています。",
            "LocalizeEN": "I am looking forward to teaching together with Sensei."
        },
        {
            "CharacterId": 10011,
            "CharacterVoiceGroupId": 10011,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_4",
            "DialogCategory": "Relationship",
            "VoiceClip": "Shun_Relationship_Up_4",
            "LocalizeJP": "この瞬間が永遠に続くといいですね。",
            "LocalizeEN": "I hope this moment lasts forever."
        }
    ]
}
