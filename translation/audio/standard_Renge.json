        {
            "CharacterId": 10082,
            "CharacterVoiceGroupId": 10082,
            "LocalizeCVGroup": "CVGroup_Tactic_In_2",
            "DialogCategory": "Tactic",
            "VoiceClip": "Renge_Tactic_In_2",
            "LocalizeJP": "さっと終わらせてくるよ！",
            "LocalizeEN": "Let's finish this!"
