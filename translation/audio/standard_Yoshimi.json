{
    "DataList": [
        {
            "CharacterId": 26005,
            "DialogCategory": "Formation",
            "VoiceClip": "Yoshimi_Formation_In_1",
            "LocalizeJP": "私が合流したことに感謝しなさい。",
            "LocalizeEN": "Be thankful that I joined you."
        },
        {
            "CharacterId": 26005,
            "DialogCategory": "Formation",
            "VoiceClip": "Yoshimi_Formation_In_2",
            "LocalizeJP": "しょうがないわね。行くわよ。",
            "LocalizeEN": "There's no other choice. Let's go."
        },
        {
            "CharacterId": 26005,
            "DialogCategory": "Battle",
            "VoiceClip": "Yoshimi_Battle_In_1",
            "LocalizeJP": "ぶっ壊してやるわ。",
            "LocalizeEN": "I'll beat the crap out of it."
        },
        {
            "CharacterId": 26005,
            "DialogCategory": "Battle",
            "VoiceClip": "Yoshimi_Battle_In_2",
            "LocalizeJP": "許せない。",
            "LocalizeEN": "I won't forgive you."
        },
        {
            "CharacterId": 26005,
            "DialogCategory": "CommonSkill",
            "VoiceClip": "Yoshimi_CommonSkill",
            "LocalizeJP": "これはどう！？",
            "LocalizeEN": "What about this!?"
        },
        {
            "CharacterId": 26005,
            "DialogCategory": "ExSkill",
            "VoiceClip": "Yoshimi_ExSkill_1",
            "LocalizeJP": "くらえ！",
            "LocalizeEN": "Take this!"
        },
        {
            "CharacterId": 26005,
            "DialogCategory": "ExSkill",
            "VoiceClip": "Yoshimi_ExSkill_2",
            "LocalizeJP": "ふんっ！やっつけてやる。",
            "LocalizeEN": "Humph! I'll beat you."
        },
        {
            "CharacterId": 26005,
            "DialogCategory": "ExSkill",
            "VoiceClip": "Yoshimi_ExSkill_3",
            "LocalizeJP": "覚悟しなさい！",
            "LocalizeEN": "Be prepared!"
        },
        {
            "CharacterId": 26005,
            "DialogCategory": "ExSkill",
            "VoiceClip": "Yoshimi_ExSkill_Level_1",
            "LocalizeJP": "はあああっ！",
            "LocalizeEN": ""
        },
        {
            "CharacterId": 26005,
            "DialogCategory": "ExSkill",
            "VoiceClip": "Yoshimi_ExSkill_Level_2",
            "LocalizeJP": "これでどう！？",
            "LocalizeEN": "How about this!?"
        },
        {
            "CharacterId": 26005,
            "DialogCategory": "ExSkill",
            "VoiceClip": "Yoshimi_ExSkill_Level_3",
            "LocalizeJP": "これでもくらえ！",
            "LocalizeEN": "Eat this as well!"
        },
        {
            "CharacterId": 26005,
            "DialogCategory": "Growup",
            "VoiceClip": "Yoshimi_Growup_1",
            "LocalizeJP": "あ、あ……ありがとう！",
            "LocalizeEN": "T-thank you!"
        },
        {
            "CharacterId": 26005,
            "DialogCategory": "Growup",
            "VoiceClip": "Yoshimi_Growup_2",
            "LocalizeJP": "ふふーん、先生！もっと私に頼ってもいいのよ。",
            "LocalizeEN": "Hehe, Sensei! It's ok to rely on me more."
        },
        {
            "CharacterId": 26005,
            "DialogCategory": "Growup",
            "VoiceClip": "Yoshimi_Growup_3",
            "LocalizeJP": "よーし！私、これでもっと強くなった。きっと背も伸びたよね？",
            "LocalizeEN": "Alright! I've grown stronger now, and I must have grown taller as well, right?"
        },
        {
            "CharacterId": 26005,
            "DialogCategory": "Growup",
            "VoiceClip": "Yoshimi_Growup_4",
            "LocalizeJP": "ありがとう、先生。いつか恩返しするから。",
            "LocalizeEN": "Thank you, Sensei. I will repay the favor someday."
        },
        {
            "CharacterId": 26005,
            "DialogCategory": "Relationship",
            "VoiceClip": "Yoshimi_Relationship_Up_1",
            "LocalizeJP": "何て言うか、その……恥ずかしいんだけど。",
            "LocalizeEN": "What can I say, um... it's kind of embarrassing."
        },
        {
            "CharacterId": 26005,
            "DialogCategory": "Relationship",
            "VoiceClip": "Yoshimi_Relationship_Up_2",
            "LocalizeJP": "今日、楽しかった。嘘じゃない。",
            "LocalizeEN": "I had fun today. That's not a lie!"
        },
        {
            "CharacterId": 26005,
            "DialogCategory": "Relationship",
            "VoiceClip": "Yoshimi_Relationship_Up_3",
            "LocalizeJP": "勘違いしないで。先生との時間が楽しすぎただけだから。何か言い間違えた気がする！？",
            "LocalizeEN": "Don't get me wrong! It's only because I had way too much fun with Sensei! I think there's something wrong with what I just said!?"
        },
        {
            "CharacterId": 26005,
            "DialogCategory": "Relationship",
            "VoiceClip": "Yoshimi_Relationship_Up_4",
            "LocalizeJP": "あ、あ……ありがとう。今はこの言葉しか浮かばないの。でも、次は、その……。",
            "LocalizeEN": "Ah, t... thank you. These are the only words that come to my mind right now. But next time, um..."
        }
    ]
}
