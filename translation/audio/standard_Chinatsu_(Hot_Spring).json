        },
        {
            "CharacterId": 10030,
            "DialogCategory": "CommonSkill",
            "VoiceClip": "Chinatsu_(Hot_Spring)_CommonSkill",
            "LocalizeJP": "バイタル、問題ありません。進撃してください。",
