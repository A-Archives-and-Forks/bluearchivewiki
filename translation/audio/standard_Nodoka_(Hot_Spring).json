        {
            "CharacterId": 20010,
            "CharacterVoiceGroupId": 20010,
            "LocalizeCVGroup": "CVGroup_Formation_In_1",
            "DialogCategory": "Formation",
            "VoiceClip": "Nodoka_(Hot_Spring)_Formation_In_1",
            "LocalizeJP": "女将の真髄、お見せします。",
            "LocalizeEN": "I will show you the essence of being a proprietress."
        },
        {
            "CharacterId": 20010,
            "CharacterVoiceGroupId": 20010,
            "LocalizeCVGroup": "CVGroup_Formation_In_2",
            "DialogCategory": "Formation",
            "VoiceClip": "Nodoka_(Hot_Spring)_Formation_In_2",
            "LocalizeJP": "ぜ、全力でおもてなしします。",
            "LocalizeEN": "I will do my best to serve you."
        },
        {
            "CharacterId": 20010,
            "CharacterVoiceGroupId": 20010,
            "LocalizeCVGroup": "CVGroup_CommonSkill",
            "DialogCategory": "CommonSkill",
            "VoiceClip": "Nodoka_(Hot_Spring)_CommonSkill",
            "LocalizeJP": "お客様、何かお困りのことはありませんか。",
            "LocalizeEN": "Is there anything I can help you with, dear customer?"
        },
        {
            "CharacterId": 20010,
            "CharacterVoiceGroupId": 20010,
            "LocalizeCVGroup": "CVGroup_ExSkill_1",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Nodoka_(Hot_Spring)_ExSkill_1",
            "LocalizeJP": "227号温泉郷の懐石料理です。",
            "LocalizeEN": "This is the [[w:kaiseki|kaiseki]] of No. 227 Hot Spring Resort."
        },
        {
            "CharacterId": 20010,
            "CharacterVoiceGroupId": 20010,
            "LocalizeCVGroup": "CVGroup_ExSkill_2",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Nodoka_(Hot_Spring)_ExSkill_2",
            "LocalizeJP": "自慢のお料理です。",
            "LocalizeEN": "It is a dish that I'm proud of."
        },
        {
            "CharacterId": 20010,
            "CharacterVoiceGroupId": 20010,
            "LocalizeCVGroup": "CVGroup_ExSkill_3",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Nodoka_(Hot_Spring)_ExSkill_3",
            "LocalizeJP": "豪華な素材がたっぷりです。",
            "LocalizeEN": "Full of luxurious ingredients."
        },
        {
            "CharacterId": 20010,
            "CharacterVoiceGroupId": 20010,
            "LocalizeCVGroup": "CVGroup_ExSkill_Level_1",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Nodoka_(Hot_Spring)_ExSkill_Level_1",
            "LocalizeJP": "お腹が空いてる方から、順番にお渡しいたします。",
            "LocalizeEN": "I will be giving out food to those who are most hungry."
        },
        {
            "CharacterId": 20010,
            "CharacterVoiceGroupId": 20010,
            "LocalizeCVGroup": "CVGroup_ExSkill_Level_2",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Nodoka_(Hot_Spring)_ExSkill_Level_2",
            "LocalizeJP": "料理はたっぷりありますので。",
            "LocalizeEN": "There's plenty of food (so please enjoy yourself)."
        },
        {
            "CharacterId": 20010,
            "CharacterVoiceGroupId": 20010,
            "LocalizeCVGroup": "CVGroup_ExSkill_Level_3",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Nodoka_(Hot_Spring)_ExSkill_Level_3",
            "LocalizeJP": "どうぞ、ごゆるりとお楽しみください。",
            "LocalizeEN": "Please enjoy yourself."
        },
        {
            "CharacterId": 20010,
            "CharacterVoiceGroupId": 20010,
            "LocalizeCVGroup": "CVGroup_Growup_1",
            "DialogCategory": "Growup",
            "VoiceClip": "Nodoka_(Hot_Spring)_Growup_1",
            "LocalizeJP": "ふふん、先生どうですか、私のこの貫禄。",
            "LocalizeEN": "What do you think, Sensei? This is my eminence."
        },
        {
            "CharacterId": 20010,
            "CharacterVoiceGroupId": 20010,
            "LocalizeCVGroup": "CVGroup_Growup_2",
            "DialogCategory": "Growup",
            "VoiceClip": "Nodoka_(Hot_Spring)_Growup_2",
            "LocalizeJP": "これで私も、立派な女将になれたでしょうか。",
            "LocalizeEN": "With this, will I be able to become a splendid proprietress?"
        },
        {
            "CharacterId": 20010,
            "CharacterVoiceGroupId": 20010,
            "LocalizeCVGroup": "CVGroup_Growup_3",
            "DialogCategory": "Growup",
            "VoiceClip": "Nodoka_(Hot_Spring)_Growup_3",
            "LocalizeJP": "温泉郷のために、もっと頑張らないと。",
            "LocalizeEN": "I have to work harder for the sake of the hot spring village."
        },
        {
            "CharacterId": 20010,
            "CharacterVoiceGroupId": 20010,
            "LocalizeCVGroup": "CVGroup_Growup_4",
            "DialogCategory": "Growup",
            "VoiceClip": "Nodoka_(Hot_Spring)_Growup_4",
            "LocalizeJP": "いつも私たち227号 特別クラ…、いえ、227号温泉郷を支援していただき、ありがとうございます。",
            "LocalizeEN": "Thank you for always supporting class No. 227... no, No. 227 Hot Spring Village."
        },
        {
            "CharacterId": 20010,
            "CharacterVoiceGroupId": 20010,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_1",
            "DialogCategory": "Relationship",
            "VoiceClip": "Nodoka_(Hot_Spring)_Relationship_Up_1",
            "LocalizeJP": "いつも先生のことを見守っていますよ～。はい、いつも～。",
            "LocalizeEN": "I'm always watching over you. Yes, always."
        },
        {
            "CharacterId": 20010,
            "CharacterVoiceGroupId": 20010,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_2",
            "DialogCategory": "Relationship",
            "VoiceClip": "Nodoka_(Hot_Spring)_Relationship_Up_2",
            "LocalizeJP": "えへへ、今日も先生のお顔は輝いてますね～。",
            "LocalizeEN": "Sensei's face is shining brightly today too."
        },
        {
            "CharacterId": 20010,
            "CharacterVoiceGroupId": 20010,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_3",
            "DialogCategory": "Relationship",
            "VoiceClip": "Nodoka_(Hot_Spring)_Relationship_Up_3",
            "LocalizeJP": "先生のために、いつも一番いいお部屋は開けてますので。",
            "LocalizeEN": "I always have the best room reserved for Sensei."
        },
        {
            "CharacterId": 20010,
            "CharacterVoiceGroupId": 20010,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_4",
            "DialogCategory": "Relationship",
            "VoiceClip": "Nodoka_(Hot_Spring)_Relationship_Up_4",
            "LocalizeJP": "先生は私たちの温泉郷におけるVIPのお客様ですから。",
            "LocalizeEN": "Sensei is the VIP guest of our hot spring village."
        }
    ]
}
