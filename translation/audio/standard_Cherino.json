        {
            "CharacterId": 10017,
            "CharacterVoiceGroupId": 10017,
            "LocalizeCVGroup": "CVGroup_ExSkill_1",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Cherino_ExSkill_1",
            "LocalizeJP": "さあ来い、親衛隊！\nあそこだ！撃て―！",
            "LocalizeEN": "Come, guards. There! Fire!"
        },
        {
            "CharacterId": 10017,
            "CharacterVoiceGroupId": 10017,
            "LocalizeCVGroup": "CVGroup_ExSkill_2",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Cherino_ExSkill_2",
            "LocalizeJP": "行け、我が親衛隊！\n粛清対象はあそこだ！",
            "LocalizeEN": "Go, my guards! There! Purge them!"
        },
        {
            "CharacterId": 10017,
            "CharacterVoiceGroupId": 10017,
            "LocalizeCVGroup": "CVGroup_ExSkill_3",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Cherino_ExSkill_3",
            "LocalizeJP": "親衛隊！\n総員、攻撃！",
            "LocalizeEN": "Guards, all hands attack!"
