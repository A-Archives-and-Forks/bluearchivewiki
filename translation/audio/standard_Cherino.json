        {
            "CharacterId": 10017,
            "CharacterVoiceGroupId": 10017,
            "LocalizeCVGroup": "CVGroup_ExSkill_Level_3",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Cherino_ExSkill_Level_3",
            "LocalizeJP": "粛清、粛清、粛清だ！",
            "LocalizeEN": "Purge them!"
