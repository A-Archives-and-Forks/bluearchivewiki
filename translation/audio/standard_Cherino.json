        {
            "CharacterId": 10017,
            "CharacterVoiceGroupId": 10017,
            "LocalizeCVGroup": "CVGroup_Battle_Victory_1",
            "DialogCategory": "Battle",
            "VoiceClip": "Cherino_Battle_Victory_1",
            "LocalizeJP": "うむ、どうだ。書記長の力、思い知ったか？\n……？あ、髭が……！",
            "LocalizeEN": "How's that? Did you realize the power of the chief secretary? ...mm? Ah! My moustache!"
        },
        {
            "CharacterId": 10017,
            "CharacterVoiceGroupId": 10017,
            "LocalizeCVGroup": "CVGroup_Battle_Victory_2",
            "DialogCategory": "Battle",
            "VoiceClip": "Cherino_Battle_Victory_2",
            "LocalizeJP": "所詮この程度か、ふははははは！\n……ん？あ、うあぁ！？",
            "LocalizeEN": "Is this what it's all about? Hahahaha! ……what? Wow!"
        },
        {
            "CharacterId": 10017,
            "CharacterVoiceGroupId": 10017,
            "LocalizeCVGroup": "CVGroup_CommonSkill",
            "DialogCategory": "CommonSkill",
            "VoiceClip": "Cherino_CommonSkill",
            "LocalizeJP": "粛清してやる！",
            "LocalizeEN": "Purge them."
