        {
            "CharacterId": 10017,
            "CharacterVoiceGroupId": 10017,
            "LocalizeCVGroup": "CVGroup_Growup_1",
            "DialogCategory": "Growup",
            "VoiceClip": "Cherino_Growup_1",
            "LocalizeJP": "この溢れ出る力……見ているか、カムラッド！",
            "LocalizeEN": "This overflowing power... are you seeing, comrade?"
        },
        {
            "CharacterId": 10017,
            "CharacterVoiceGroupId": 10017,
            "LocalizeCVGroup": "CVGroup_Growup_2",
            "DialogCategory": "Growup",
            "VoiceClip": "Cherino_Growup_2",
            "LocalizeJP": "悪くない、悪くないぞ……おいらの成長に限界はない！",
            "LocalizeEN": "Not bad, not bad, there's no limit to my growth!"
        },
        {
            "CharacterId": 10017,
            "CharacterVoiceGroupId": 10017,
            "LocalizeCVGroup": "CVGroup_Growup_3",
            "DialogCategory": "Growup",
            "VoiceClip": "Cherino_Growup_3",
            "LocalizeJP": "これぞまさに、レッドウィンターの力だ！",
            "LocalizeEN": "This is the true power of Red Winter."
