        {
            "CharacterId": 10017,
            "CharacterVoiceGroupId": 10017,
            "LocalizeCVGroup": "CVGroup_ExSkill_Level_1",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Cherino_ExSkill_Level_1",
            "LocalizeJP": "全部やっつけてしまえ！",
            "LocalizeEN": "Get rid of them all."
        },
        {
            "CharacterId": 10017,
            "CharacterVoiceGroupId": 10017,
            "LocalizeCVGroup": "CVGroup_ExSkill_Level_2",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Cherino_ExSkill_Level_2",
            "LocalizeJP": "よし！攻撃だ！",
            "LocalizeEN": "All right, attack."
