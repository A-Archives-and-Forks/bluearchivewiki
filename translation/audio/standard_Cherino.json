            "CharacterVoiceGroupId": 10017,
            "LocalizeCVGroup": "CVGroup_Formation_Select",
            "DialogCategory": "Formation",
            "VoiceClip": "Cherino_Formation_Select",
            "LocalizeJP": "あ、あれっ！？は、離せ！",
