            "CharacterId": 10017,
            "CharacterVoiceGroupId": 10017,
            "LocalizeCVGroup": "CVGroup_Growup_4",
            "DialogCategory": "Growup",
            "VoiceClip": "Cherino_Growup_4",
            "LocalizeJP": "やるではないか、カムラッド！",
            "LocalizeEN": "Well done, comrade."
