{
    "DataList": [
        {
            "CharacterId": 26001,
            "CharacterVoiceGroupId": 26001,
            "LocalizeCVGroup": "CVGroup_Formation_In_1",
            "DialogCategory": "Formation",
            "VoiceClip": "Kotama_Formation_In_1",
            "LocalizeJP": "素晴らしい判断です。",
            "LocalizeEN": "Great judgment."
        },
        {
            "CharacterId": 26001,
            "CharacterVoiceGroupId": 26001,
            "LocalizeCVGroup": "CVGroup_Formation_In_2",
            "DialogCategory": "Formation",
            "VoiceClip": "Kotama_Formation_In_2",
            "LocalizeJP": "適切な選択だと考えます。",
            "LocalizeEN": "I think it's the right choice"
        },
        {
            "CharacterId": 26001,
            "CharacterVoiceGroupId": 26001,
            "LocalizeCVGroup": "CVGroup_Battle_In_1",
            "DialogCategory": "Battle",
            "VoiceClip": "Kotama_Battle_In_1",
            "LocalizeJP": "戦闘を開始します。",
            "LocalizeEN": "Combat begins."
        },
        {
            "CharacterId": 26001,
            "CharacterVoiceGroupId": 26001,
            "LocalizeCVGroup": "CVGroup_Battle_In_2",
            "DialogCategory": "Battle",
            "VoiceClip": "Kotama_Battle_In_2",
            "LocalizeJP": "作戦開始。敵を探知します。",
            "LocalizeEN": "Operation start. Detecting the enemy."
        },
        {
            "CharacterId": 26001,
            "CharacterVoiceGroupId": 26001,
            "LocalizeCVGroup": "CVGroup_CommonSkill",
            "DialogCategory": "CommonSkill",
            "VoiceClip": "Kotama_CommonSkill",
            "LocalizeJP": "貴方の狙いはすべて把握しました。",
            "LocalizeEN": "I understand your aim."
        },
        {
            "CharacterId": 26001,
            "CharacterVoiceGroupId": 26001,
            "LocalizeCVGroup": "CVGroup_ExSkill_1",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Kotama_ExSkill_1",
            "LocalizeJP": "傍受開始。",
            "LocalizeEN": "Interception started."
        },
        {
            "CharacterId": 26001,
            "CharacterVoiceGroupId": 26001,
            "LocalizeCVGroup": "CVGroup_ExSkill_2",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Kotama_ExSkill_2",
            "LocalizeJP": "しっ、傍受を始めます。",
            "LocalizeEN": "…be quiet. Start intercepting."
        },
        {
            "CharacterId": 26001,
            "CharacterVoiceGroupId": 26001,
            "LocalizeCVGroup": "CVGroup_ExSkill_3",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Kotama_ExSkill_3",
            "LocalizeJP": "私の耳は誤魔化せません。",
            "LocalizeEN": "My ears can't be fooled."
        },
        {
            "CharacterId": 26001,
            "CharacterVoiceGroupId": 26001,
            "LocalizeCVGroup": "CVGroup_ExSkill_Level_1",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Kotama_ExSkill_Level_1",
            "LocalizeJP": "……キャッチ。",
            "LocalizeEN": "Catch."
        },
        {
            "CharacterId": 26001,
            "CharacterVoiceGroupId": 26001,
            "LocalizeCVGroup": "CVGroup_ExSkill_Level_2",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Kotama_ExSkill_Level_2",
            "LocalizeJP": "……見つけました。",
            "LocalizeEN": "I found."
        },
        {
            "CharacterId": 26001,
            "CharacterVoiceGroupId": 26001,
            "LocalizeCVGroup": "CVGroup_ExSkill_Level_3",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Kotama_ExSkill_Level_3",
            "LocalizeJP": "……エンカウント。リンクします。",
            "LocalizeEN": "Encounter link."
        },
        {
            "CharacterId": 26001,
            "CharacterVoiceGroupId": 26001,
            "LocalizeCVGroup": "CVGroup_Growup_1",
            "DialogCategory": "Growup",
            "VoiceClip": "Kotama_Growup_1",
            "LocalizeJP": "この調和された力は……まるで洗練されたオーケストラのようですね。",
            "LocalizeEN": "This harmonious force is like a sophisticated orchestra."
        },
        {
            "CharacterId": 26001,
            "CharacterVoiceGroupId": 26001,
            "LocalizeCVGroup": "CVGroup_Growup_2",
            "DialogCategory": "Growup",
            "VoiceClip": "Kotama_Growup_2",
            "LocalizeJP": "これで私はもっと強くなりました。試してもらっても構いません。",
            "LocalizeEN": "Now I'm stronger, you can try it."
        },
        {
            "CharacterId": 26001,
            "CharacterVoiceGroupId": 26001,
            "LocalizeCVGroup": "CVGroup_Growup_3",
            "DialogCategory": "Growup",
            "VoiceClip": "Kotama_Growup_3",
            "LocalizeJP": "この滑らかな旋律……もっと聞きたいです。",
            "LocalizeEN": "I want to hear more of this smooth melody."
        },
        {
            "CharacterId": 26001,
            "CharacterVoiceGroupId": 26001,
            "LocalizeCVGroup": "CVGroup_Growup_4",
            "DialogCategory": "Growup",
            "VoiceClip": "Kotama_Growup_4",
            "LocalizeJP": "これでもっとよく聞こえます。……先生の心拍の音まで。",
            "LocalizeEN": "Now I can hear better, even including the sound of Sensei's heartbeat."
        },
        {
            "CharacterId": 26001,
            "CharacterVoiceGroupId": 26001,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_1",
            "DialogCategory": "Relationship",
            "VoiceClip": "Kotama_Relationship_Up_1",
            "LocalizeJP": "心拍数上昇、血流加速、体温上昇。原因は……不明。",
            "LocalizeEN": "Increased heart rate, increased blood flow, increased body temperature, cause unknown."
        },
        {
            "CharacterId": 26001,
            "CharacterVoiceGroupId": 26001,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_2",
            "DialogCategory": "Relationship",
            "VoiceClip": "Kotama_Relationship_Up_2",
            "LocalizeJP": "今まで先生と交わした、すべての会話は録音してあります。もしかして……聞きたいですか?",
            "LocalizeEN": "All conversations I have had with Sensei so far have been recorded. Maybe...do you want to hear it?"
        },
        {
            "CharacterId": 26001,
            "CharacterVoiceGroupId": 26001,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_3",
            "DialogCategory": "Relationship",
            "VoiceClip": "Kotama_Relationship_Up_3",
            "LocalizeJP": "先生の声は私にとって音楽と同じです。私の心を高揚させますから。",
            "LocalizeEN": "Sensei's voice is like music to me because it lifts my spirits."
        },
        {
            "CharacterId": 26001,
            "CharacterVoiceGroupId": 26001,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_4",
            "DialogCategory": "Relationship",
            "VoiceClip": "Kotama_Relationship_Up_4",
            "LocalizeJP": "……確信しました。先生は私にとって特別です。",
            "LocalizeEN": "Confirmed. Sensei is special to me."
        }
    ]
}
