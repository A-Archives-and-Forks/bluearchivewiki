{
    "DataList": [
        {
            "CharacterId": 20013,
            "CharacterVoiceGroupId": 20013,
            "LocalizeCVGroup": "CVGroup_Formation_In_1",
            "DialogCategory": "Formation",
            "VoiceClip": "Chihiro_Formation_In_1",
            "LocalizeJP": "ん？新しいセキュリティホールでも見つかった？",
            "LocalizeEN": "Did you find a new security hole or something?"
        },
        {
            "CharacterId": 20013,
            "CharacterVoiceGroupId": 20013,
            "LocalizeCVGroup": "CVGroup_Formation_In_2",
            "DialogCategory": "Formation",
            "VoiceClip": "Chihiro_Formation_In_2",
            "LocalizeJP": "了解。すぐに行く。",
            "LocalizeEN": "Got it. Be there in a sec."
        },
        {
            "CharacterId": 20013,
            "CharacterVoiceGroupId": 20013,
            "LocalizeCVGroup": "CVGroup_CommonSkill",
            "DialogCategory": "CommonSkill",
            "VoiceClip": "Chihiro_CommonSkill",
            "LocalizeJP": "セキュリティ上の弱点、修復するね。",
            "LocalizeEN": "I'll fix any security vulnerabilities."
        },
        {
            "CharacterId": 20013,
            "CharacterVoiceGroupId": 20013,
            "LocalizeCVGroup": "CVGroup_ExSkill_1",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Chihiro_ExSkill_1",
            "LocalizeJP": "こういうクラッキングは好みじゃないけど。",
            "LocalizeEN": "Though I'm not fond of this type of cracking."
        },
        {
            "CharacterId": 20013,
            "CharacterVoiceGroupId": 20013,
            "LocalizeCVGroup": "CVGroup_ExSkill_2",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Chihiro_ExSkill_2",
            "LocalizeJP": "可愛い後輩たちのためなら、仕方ない。",
            "LocalizeEN": "Can't say no if it's for my cute juniors."
        },
        {
            "CharacterId": 20013,
            "CharacterVoiceGroupId": 20013,
            "LocalizeCVGroup": "CVGroup_ExSkill_3",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Chihiro_ExSkill_3",
            "LocalizeJP": "はあ、こういうのは今回だけだよ。",
            "LocalizeEN": "I'm only doing it this time."
        },
        {
            "CharacterId": 20013,
            "CharacterVoiceGroupId": 20013,
            "LocalizeCVGroup": "CVGroup_ExSkill_Level_1",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Chihiro_ExSkill_Level_1",
            "LocalizeJP": "もう少しセキュリティ意識を持った方がいい。",
            "LocalizeEN": "You should be a little more security conscious."
        },
        {
            "CharacterId": 20013,
            "CharacterVoiceGroupId": 20013,
            "LocalizeCVGroup": "CVGroup_ExSkill_Level_2",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Chihiro_ExSkill_Level_2",
            "LocalizeJP": "うん、全部見えた。",
            "LocalizeEN": "I see everything."
        },
        {
            "CharacterId": 20013,
            "CharacterVoiceGroupId": 20013,
            "LocalizeCVGroup": "CVGroup_ExSkill_Level_3",
            "DialogCategory": "ExSkill",
            "VoiceClip": "Chihiro_ExSkill_Level_3",
            "LocalizeJP": "",
            "LocalizeEN": ""
        },
        {
            "CharacterId": 20013,
            "CharacterVoiceGroupId": 20013,
            "LocalizeCVGroup": "CVGroup_Growup_1",
            "DialogCategory": "Growup",
            "VoiceClip": "Chihiro_Growup_1",
            "LocalizeJP": "うん。やっぱり、学びに限界は存在しないものだ。",
            "LocalizeEN": "OK. There are no limits to learning after all."
        },
        {
            "CharacterId": 20013,
            "CharacterVoiceGroupId": 20013,
            "LocalizeCVGroup": "CVGroup_Growup_2",
            "DialogCategory": "Growup",
            "VoiceClip": "Chihiro_Growup_2",
            "LocalizeJP": "先生がくれる知識はいつも目新しいものばかりだね。",
            "LocalizeEN": "With Sensei I'm always learning novel things."
        },
        {
            "CharacterId": 20013,
            "CharacterVoiceGroupId": 20013,
            "LocalizeCVGroup": "CVGroup_Growup_3",
            "DialogCategory": "Growup",
            "VoiceClip": "Chihiro_Growup_3",
            "LocalizeJP": "ありがとう、先生。また少し賢くなれた。",
            "LocalizeEN": "Thanks, Sensei. I've become a little smarter again."
        },
        {
            "CharacterId": 20013,
            "CharacterVoiceGroupId": 20013,
            "LocalizeCVGroup": "CVGroup_Growup_4",
            "DialogCategory": "Growup",
            "VoiceClip": "Chihiro_Growup_4",
            "LocalizeJP": "へえ…、いいデータだね。",
            "LocalizeEN": "Hmm, good data."
        },
        {
            "CharacterId": 20013,
            "CharacterVoiceGroupId": 20013,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_1",
            "DialogCategory": "Relationship",
            "VoiceClip": "Chihiro_Relationship_Up_1",
            "LocalizeJP": "先生に対してハッキングとかはしたくないから。思ってることは素直に教えて欲しい。",
            "LocalizeEN": "I don't want to hack you or anything, so... I want you to be honest with me."
        },
        {
            "CharacterId": 20013,
            "CharacterVoiceGroupId": 20013,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_2",
            "DialogCategory": "Relationship",
            "VoiceClip": "Chihiro_Relationship_Up_2",
            "LocalizeJP": "私はいつもここから先生のことを支援してるから。",
            "LocalizeEN": "I'll always support you from over here, Sensei."
        },
        {
            "CharacterId": 20013,
            "CharacterVoiceGroupId": 20013,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_3",
            "DialogCategory": "Relationship",
            "VoiceClip": "Chihiro_Relationship_Up_3",
            "LocalizeJP": "先生みたいに甘っちょろいのはどうにも放っておけなくて。…あくまでセキュリティ的な話。",
            "LocalizeEN": "I can't just leave someone naive and easy-going like Sensei alone. I'm strictly talking in a security sense."
        },
        {
            "CharacterId": 20013,
            "CharacterVoiceGroupId": 20013,
            "LocalizeCVGroup": "CVGroup_Relationship_Up_4",
            "DialogCategory": "Relationship",
            "VoiceClip": "Chihiro_Relationship_Up_4",
            "LocalizeJP": "先生、その…、今夜、深夜のセキュリティ点検に行ってもいい？",
            "LocalizeEN": "Sensei, um... can you come to the late-night security check tonight?"
        }
    ]
}
